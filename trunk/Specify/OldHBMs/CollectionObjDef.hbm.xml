<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.CollectionObjDef" table="collectionobjdef" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="collectionObjDefId" type="java.lang.Long" column="CollectionObjDefID">
      <meta attribute="scope-set">public</meta>
      <meta attribute="scope-field">protected</meta>
      <generator class="native"/>
    </id>

    <timestamp name="timestampModified" column="TimestampModified">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Versioning Timestamp</meta>
    </timestamp>

    <property name="name" type="java.lang.String" column="Name" length="64">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="field-description"></meta>
    </property>
    
    <property name="discipline" type="java.lang.String" column="Discipline" length="64">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="field-description"></meta>
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <!-- Associations -->

    <!-- many-to-one association to DataType -->
    <many-to-one name="dataType" class="edu.ku.brc.specify.datamodel.DataType" outer-join="true" cascade="save-update, lock, merge">
    	<meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <column name="DataTypeID"  not-null="true"/>
    </many-to-one>
    
    <!-- bi-directional many-to-many association to CatalogSeries -->
    <set name="catalogSeries" table="catseries_colobjdef" lazy="true" inverse="true" cascade="all-delete-orphan">
        <key>
            <column name="CollectionObjDefID"/>
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.CatalogSeries" column="CatalogSeriesID"/>
    </set>
        
    <!-- many-to-one association to SpecifyUser -->
    <many-to-one name="specifyUser" class="edu.ku.brc.specify.datamodel.SpecifyUser" outer-join="true" cascade="save-update, lock, merge">
    	<meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <column name="SpecifyUserID"  not-null="true"/>
    </many-to-one>   
    
    <!-- bi-directional one-to-many association to AttributeDef -->
    <set name="attributeDefs" lazy="true" inverse="true" cascade="all-delete-orphan">
        <meta attribute="field-description"></meta>
        <key>
            <column name="CollectionObjDefID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.AttributeDef"/>
    </set>
    
    <!-- bi-directional many-to-one association to GeographyTreeDef -->
    <many-to-one name="geographyTreeDef" class="edu.ku.brc.specify.datamodel.GeographyTreeDef" outer-join="true" cascade="save-update, lock, merge">
        <meta attribute="field-description"></meta>
        <column name="GeographyTreeDefID"  not-null="true"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to GeologicTimePeriodTreeDef -->
    <many-to-one name="geologicTimePeriodTreeDef" class="edu.ku.brc.specify.datamodel.GeologicTimePeriodTreeDef" outer-join="true" cascade="save-update, lock, merge">
        <meta attribute="field-description"></meta>
        <column name="GeologicTimePeriodTreeDefID"  not-null="true"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to LocationTreeDef -->
    <many-to-one name="locationTreeDef" class="edu.ku.brc.specify.datamodel.LocationTreeDef" outer-join="true" cascade="save-update, lock, merge">
        <meta attribute="field-description"></meta>
        <column name="LocationTreeDefID"  not-null="true"/>
    </many-to-one>
    
    <!-- bi-directional one-to-one association to TaxonTreeDef -->
    <one-to-one name="taxonTreeDef" class="edu.ku.brc.specify.datamodel.TaxonTreeDef" cascade="all">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">@hibernate.one-to-one</meta>
    </one-to-one>
    
    <!-- bi-directional many-to-many association to Locality -->
    <set name="localities" table="colobjdef_locality" lazy="true" inverse="true" cascade="save-update, lock, merge">
        <key>
            <column name="CollectionObjDefID"/>
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.Locality" column="LocalityID"/>
    </set>
    
    <!-- bi-directional one-to-many association to AppResourceDefault -->
    <set name="appResourceDefaults" lazy="true" inverse="true" cascade="none">
        <meta attribute="field-description"></meta>
        <key>
            <column name="CollectionObjDefID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.AppResourceDefault"/>
    </set>    
    
    <!-- bi-directional one-to-many association to CollectionObject -->
	<set name="collectionObjects" lazy="true" inverse="true" cascade="all-delete-orphan">
        <meta attribute="field-description"></meta>
        <key>
        	<column name="CollectionObjDefID" />
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.CollectionObject"/>
    </set>
 
    <!-- bi-directional one-to-many association to UserPermission -->
    <set name="userPermissions" lazy="true" inverse="true" cascade="all-delete-orphan">
        <meta attribute="field-description"></meta>
        <key>
            <column name="CollectionObjDefID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.UserPermission"/>
    </set> 
          
</class>
</hibernate-mapping>
