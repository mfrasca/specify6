<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.UserPermission" table="sp_userpermission" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="userPermissionId" type="java.lang.Long" column="UserPermissionID">
      <meta attribute="scope-set">public</meta>
      <meta attribute="scope-field">protected</meta>
      <generator class="native"/>
    </id>
  
    <property name="dataAccessPrivilege" type="boolean" column="DataAccessPrivilege">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="adminPrivilege" type="boolean" column="AdminPrivilege">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>  

    <!-- Associations -->
    
    <!-- bi-directional one-to-many association to SpecifyUser -->
    
    
        <!-- many-to-one association to SpecifyUser -->
        <many-to-one name="specifyUser" class="edu.ku.brc.specify.datamodel.SpecifyUser" outer-join="true">
            <meta attribute="scope-field">protected</meta>
            <meta attribute="field-description"></meta>
            <column name="SpecifyUserID" not-null="true"/>
        </many-to-one>         
    
            <!-- many-to-one association to CollectionObjDef -->
        <many-to-one name="collectionObjDef" class="edu.ku.brc.specify.datamodel.CollectionObjDef" outer-join="true">
            <meta attribute="scope-field">protected</meta>
            <meta attribute="field-description"></meta>
            <column name="CollectionObjDefID" not-null="true"/>
        </many-to-one> 
       
</class>
</hibernate-mapping>
