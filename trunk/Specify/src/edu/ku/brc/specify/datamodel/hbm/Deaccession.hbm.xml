<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.Deaccession" table="deaccession" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="deaccessionId" type="java.lang.Integer" column="DeaccessionID">
        <meta attribute="field-description">Primary key</meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>

    <property name="type" type="java.lang.String" column="Type" length="64">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Description of the Type of deaccession; i.e. Gift, disposal, lost</meta>
    </property>
    
    <property name="deaccessionNumber" type="java.lang.String" column="DeaccessionNumber" length="50" index="IX_DeaccessionNumber">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Name institution assigns to the deacession</meta>
    </property>
    
    <property name="deaccessionDate" type="calendar_date" column="DeaccessionDate">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="text1" type="text" column="Text1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="text2" type="text" column="Text2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="number1" type="java.lang.Float" column="Number1" length="24">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="number2" type="java.lang.Float" column="Number2" length="24">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="timestampModified" type="java.sql.Timestamp" column="TimestampModified" length="23">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="yesNo1" type="boolean" column="YesNo1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="yesNo2" type="boolean" column="YesNo2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>


    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to DeaccessionAgents -->
    <set name="deaccessionAgents" lazy="true" inverse="true" cascade="delete">
        <meta attribute="field-description"></meta>
        <key>
            <column name="DeaccessionID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.DeaccessionAgents"/>
    </set>
    
    <!-- bi-directional one-to-many association to DeaccessionCollectionObject -->
    <set name="deaccessionCollectionObjects" lazy="true" inverse="true" cascade="delete">
        <meta attribute="field-description"></meta>
        <key>
            <column name="DeaccessionID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.DeaccessionCollectionObject"/>
    </set>
    

</class>
</hibernate-mapping>
