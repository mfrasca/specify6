                        throw new IllegalArgumentException("Argument must be an instanceof TaxonTreeDef");
                    }
                    setTreeDef((TaxonTreeDef)treeDef);
                }
                
                public TreeDefinitionItemIface getParentItem()
                {
                    return getParent();
                }
                
