<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.ProjectCollectionObject" table="projectcollectionobjects" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="projectCollectionObjectId" type="java.lang.Long" column="ProjectCollectionObjectID">
        <meta attribute="field-description"></meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>
    
    <timestamp name="timestampModified" column="TimestampModified">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Versioning Timestamp</meta>
    </timestamp>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to CollectionObject -->
    <many-to-one name="collectionObject" class="edu.ku.brc.specify.datamodel.CollectionObject"  cascade="none" outer-join="true" unique-key="IX_ProjectCollectionObject">
        <meta attribute="field-description"></meta>
        <column name="CollectionObjectID" not-null="true"/>
    </many-to-one>
    
    
    <!-- bi-directional many-to-one association to Project -->
    <many-to-one name="project" class="edu.ku.brc.specify.datamodel.Project" outer-join="true" cascade="none" index="IX_ProjectCollectionObjectProjectID" unique-key="IX_ProjectCollectionObject">
        <meta attribute="field-description"></meta>
        <column name="ProjectID"  not-null="true"/>
    </many-to-one>

</class>
</hibernate-mapping>
