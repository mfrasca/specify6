<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.BorrowShipments" table="borrowshipments" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="borrowShipmentsId" type="java.lang.Integer" column="BorrowShipmentsID">
        <meta attribute="field-description"></meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampModified" type="java.sql.Timestamp" column="TimestampModified" length="23">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

    <!-- Associations -->
		<!-- this was missing but existing in original mapping file - now replaced -->
    <!-- bi-directional many-to-one association to Shipment -->
    <many-to-one name="shipment" class="edu.ku.brc.specify.datamodel.Shipment" not-null="true" index="IX_BorrowShipmentsShipmentID" unique-key="IX_BorrowShipments">
        <meta attribute="field-description">The shipment</meta>
        <column name="ShipmentID"/>
    </many-to-one>
	
    <!-- bi-directional many-to-one association to Borrow -->
    <many-to-one name="borrow" class="edu.ku.brc.specify.datamodel.Borrow" not-null="true" outer-join="true" cascade="none" index="IX_BorrowShipmentsBorrowID" unique-key="IX_BorrowShipments">
        <meta attribute="field-description">The borrow being shipped (returned)</meta>
        <column name="BorrowID"/>
    </many-to-one>

</class>
</hibernate-mapping>
