<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="edu.ku.brc.specify.datamodel.InfoRequest" 
    table="inforequest"
	lazy="false"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="inforequest"
    </meta>

	<id name="infoRequestID"
		type="long"
		column="infoRequestID"
		unsaved-value="null">
		<generator class="native"/>
	</id>

    <property
        name="firstName"
        type="java.lang.String"
        column="firstname"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="firstname"
            length="50"
        </meta>    
    </property>
    
    <property
        name="lastName"
        type="java.lang.String"
        column="lastname"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="lastname"
            length="50"
        </meta>    
    </property>
    
    <property
        name="institution"
        type="java.lang.String"
        column="institution"
        length="127"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="institution"
            length="127"
        </meta>    
    </property>
    
    <property
        name="email"
        type="java.lang.String"
        column="email"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="email"
            length="50"
        </meta>    
    </property>
    
    <property
        name="requestDate"
        type="java.util.Date"
        column="requestdate"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="requestdate"
            update="false"
         </meta>    
    </property>
    
    <property
        name="replyDate"
        type="java.util.Date"
        column="replydate"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="replydate"
         </meta>    
    </property>
    
    <property
        name="remarks"
        type="java.lang.String"
        column="Remarks"
        length="1073741823"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
            length="1073741823"
        </meta>    
    </property>
    
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
        </meta>    
    </property>
    
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
        </meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to RecordSet -->
    <many-to-one
        name="recordSet"
        class="edu.ku.brc.specify.datamodel.RecordSet"
        not-null="true"
		outer-join="auto"
		lazy="false"
    >
	<meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="id"         
        </meta>
        <column name="id" />
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Agent -->

    <many-to-one
        name="agent"
        class="edu.ku.brc.specify.datamodel.Agent"
        not-null="false"
		outer-join="auto"
	    lazy="false"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="false"
           @hibernate.column name="agentId"         
        </meta>
        <column name="agentId" />
    </many-to-one>

</class>
</hibernate-mapping>
