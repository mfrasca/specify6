<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.PreparationAttr" table="preparationattr" lazy="true">
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="preparationattr"
    </meta>
    <meta attribute="implements">AttributeIFace</meta>
 
    <id name="attrId" type="java.lang.Integer" column="AttrID">
      <meta attribute="scope-set">public</meta>
      <meta attribute="scope-field">protected</meta>
      <generator class="native"/>
    </id>
	
    <property name="strValue" type="java.lang.String" column="StrValue" length="255">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

    <property name="dblValue" type="double" column="DoubleValue">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampModified" type="java.sql.Timestamp" column="TimestampModified" length="23">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
	
	<!-- Associations -->

    <!-- bi-directional many-to-one association to AttributeDef -->
    <many-to-one name="definition" class="edu.ku.brc.specify.datamodel.AttributeDef" not-null="true" outer-join="true">
	<meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <column name="AttributeDefID"/>
    </many-to-one>	
    	
    <!-- bi-directional many-to-one association to Preparation -->
    <many-to-one name="preparation" class="edu.ku.brc.specify.datamodel.Preparation" not-null="true" outer-join="true">
        <meta attribute="field-description"></meta>
        <column name="PreparationId"/>
    </many-to-one>	

</class>

</hibernate-mapping>