<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.Shipment" table="shipment" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="shipmentId" type="java.lang.Integer" column="ShipmentID">
        <meta attribute="field-description">Primary key</meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>

    <property name="shipmentDate" type="calendar_date" column="ShipmentDate">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Date of shipment</meta>
    </property>
    
    <property name="shipmentNumber" type="java.lang.String" column="ShipmentNumber" not-null="true" length="50" index="IX_Shipment">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Shipper's tracking number</meta>
    </property>
    
    <property name="shipmentMethod" type="java.lang.String" column="ShipmentMethod" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Description of shipment. E.g. 'Hand-carried', 'Overnight', 'Air', 'Land', 'Sea', ...</meta>
    </property>
    
    <property name="numberOfPackages" type="short" column="NumberOfPackages">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Number of packages shipped</meta>
    </property>
    
    <property name="weight" type="java.lang.String" column="Weight" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">The weight of the shipment</meta>
    </property>
    
    <property name="insuredForAmount" type="java.lang.String" column="InsuredForAmount" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="text1" type="text" column="Text1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="text2" type="text" column="Text2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="number1" type="java.lang.Float" column="Number1" length="24">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="number2" type="java.lang.Float" column="Number2" length="24">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampModified" type="java.sql.Timestamp" column="TimestampModified" length="23">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

    <property name="yesNo1" type="boolean" column="YesNo1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="yesNo2" type="boolean" column="YesNo2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>



    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one name="AgentByShipper" class="edu.ku.brc.specify.datamodel.Agent" not-null="true" outer-join="true">
        <meta attribute="field-description">AgentID of agent transporting the material</meta>
        <column name="ShipperID"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one name="AgentByShippedTo" class="edu.ku.brc.specify.datamodel.Agent" not-null="true" outer-join="true">
        <meta attribute="field-description">AgentID of agent material is shipped to</meta>
        <column name="ShippedToID"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one name="agent" class="edu.ku.brc.specify.datamodel.Agent" not-null="true" outer-join="true">
        <meta attribute="field-description">AgentID of person approving/initiating the shipment</meta>
        <column name="ShippedByID"/>
    </many-to-one>
	
	<!-- this was missing but existing in original mapping file - now replaced -->
    <!-- bi-directional one-to-many association to BorrowShipment -->
    <set name="borrowShipments" lazy="true" inverse="true" cascade="none">
        <meta attribute="field-description"></meta>
        <key>
            <column name="ShipmentID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.BorrowShipment"/>
    </set>
	
    <!-- bi-directional one-to-many association to Loan -->
    <set name="loans" lazy="true" inverse="true" cascade="none">
        <meta attribute="field-description"></meta>
        <key>
            <column name="ShipmentID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.Loan"/>
    </set>
    
    <!-- bi-directional one-to-many association to ExchangeOut -->
    <set name="exchangeOuts" lazy="true" inverse="true" cascade="none">
        <meta attribute="field-description"></meta>
        <key>
            <column name="ShipmentID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.ExchangeOut"/>
    </set>

</class>
</hibernate-mapping>
