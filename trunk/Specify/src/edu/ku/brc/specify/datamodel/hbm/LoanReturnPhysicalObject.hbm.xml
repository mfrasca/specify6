<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.LoanReturnPhysicalObject" table="loanreturnphysicalobject" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="loanReturnPhysicalObjectId" type="java.lang.Long" column="LoanReturnPhysicalObjectID">
        <meta attribute="field-description">PrimaryKey</meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>

    <timestamp name="timestampModified" column="TimestampModified">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Versioning Timestamp</meta>
    </timestamp>

    <property name="returnedDate" type="calendar_date" column="ReturnedDate">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="quantity" type="java.lang.Integer" column="Quantity">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Quantity of items returned (necessary for lots)</meta>
    </property>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Date the record was created</meta>
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">user ID  of the person last editing the record</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to LoanPhysicalObject -->
    <many-to-one name="loanPhysicalObject" class="edu.ku.brc.specify.datamodel.LoanPhysicalObject" outer-join="true" cascade="none" index="IX_LoanReturnPhysicalObjectLoanPhysicalObjectID">
        <meta attribute="field-description">Link to LoanPhysicalObject table</meta>
        <column name="LoanPhysicalObjectID"  not-null="true"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to DeaccessionCollectionObject not-null="true"-->
    <!-- <many-to-one name="deaccessionCollectionObject" class="edu.ku.brc.specify.datamodel.DeaccessionCollectionObject" cascade="none" outer-join="true">
        <meta attribute="field-description">ID of associated (if present) DeaccessionPhysicalObject record</meta>
        <column name="DeaccessionPhysicalObjectID"  />
    </many-to-one> -->
    
     <!-- bi-directional many-to-one association to DeaccessionPreparation not-null="true"-->
    <many-to-one name="deaccessionPreparation" class="edu.ku.brc.specify.datamodel.DeaccessionPreparation" cascade="none" outer-join="true">
        <meta attribute="field-description">ID of associated (if present) DeaccessionPhysicalObject record</meta>
        <column name="DeaccessionPhysicalObjectID"  />
    </many-to-one>   
    
    <!-- bi-directional many-to-one association to Agent  not-null="true"-->
    <many-to-one name="receivedBy" class="edu.ku.brc.specify.datamodel.Agent" cascade="save-update, lock, merge" outer-join="true">
        <meta attribute="field-description">Person processing the loan return</meta>
        <column name="ReceivedByID" />
    </many-to-one>

</class>
</hibernate-mapping>
