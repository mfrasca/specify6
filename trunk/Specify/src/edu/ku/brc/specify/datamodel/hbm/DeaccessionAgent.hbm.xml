<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.DeaccessionAgent" table="deaccessionagent" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="deaccessionAgentId" type="java.lang.Long" column="DeaccessionAgentID">
        <meta attribute="field-description"></meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>

    <timestamp name="timestampModified" column="TimestampModified">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Versioning Timestamp</meta>
    </timestamp>

    <property name="role" type="java.lang.String" column="Role" not-null="true" length="32" unique-key="IX_DeaccessionAgent">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Role agent played in deaccession</meta>
    </property>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one name="Agent" class="edu.ku.brc.specify.datamodel.Agent" cascade="save-update"  outer-join="true" index="IX_DeaccessionAgentAgentID" unique-key="IX_DeaccessionAgent">
        <meta attribute="field-description">AgentID for agent</meta>
        <column name="AgentID"  not-null="true"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Deaccession -->
    <many-to-one name="deaccession" class="edu.ku.brc.specify.datamodel.Deaccession" outer-join="true" cascade="none" index="IX_DeaccessionAgentDeaccessionID" unique-key="IX_DeaccessionAgent">
        <meta attribute="field-description">Deaccession agent played role in</meta>
        <column name="DeaccessionID"  not-null="true"/>
    </many-to-one>

</class>
</hibernate-mapping>
