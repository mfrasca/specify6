<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="edu.ku.brc.specify.datamodel.Shipment" 
    table="shipment"
	lazy="true"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="shipment"
    </meta>

    <id
        name="shipmentId"
        type="java.lang.Integer"
        column="ShipmentID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Integer"
            column="ShipmentID"

        </meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="assigned" />
    </id>

    <property name="shipmentDate"
        type="calendar_date"
        column="ShipmentDate"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="ShipmentDate"
        </meta>    
    </property>
    
    <property
        name="shipmentNumber"
        type="java.lang.String"
        column="ShipmentNumber"
        not-null="true"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="ShipmentNumber"
            length="50"
            not-null="true"
        </meta>    
    </property>
    <property
        name="shipmentMethod"
        type="java.lang.String"
        column="ShipmentMethod"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="ShipmentMethod"
            length="50"
        </meta>    
    </property>
    <property
        name="numberOfPackages"
        type="short"
        column="NumberOfPackages"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="NumberOfPackages"
        </meta>    
    </property>
    <property
        name="weight"
        type="java.lang.String"
        column="Weight"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Weight"
            length="50"
        </meta>    
    </property>
    <property
        name="insuredForAmount"
        type="java.lang.String"
        column="InsuredForAmount"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="InsuredForAmount"
            length="50"
        </meta>    
    </property>
    
    <property
        name="remarks"
        type="text"
        column="Remarks"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
        </meta>    
    </property>
    
    <property
        name="text1"
        type="java.lang.String"
        column="Text1"
        length="300"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Text1"
            length="300"
        </meta>    
    </property>
    <property
        name="text2"
        type="java.lang.String"
        column="Text2"
        length="300"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Text2"
            length="300"
        </meta>    
    </property>
    <property
        name="number1"
        type="java.lang.Float"
        column="Number1"
        length="24"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Number1"
            length="24"
        </meta>    
    </property>
    <property
        name="number2"
        type="java.lang.Float"
        column="Number2"
        length="24"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Number2"
            length="24"
        </meta>    
    </property>
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
        </meta>    
    </property>
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
        </meta>    
    </property>
    <property
        name="lastEditedBy"
        type="java.lang.String"
        column="LastEditedBy"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="LastEditedBy"
            length="50"
        </meta>    
    </property>
    <property
        name="shipmentMethodId"
        type="java.lang.Integer"
        column="ShipmentMethodID"
        length="10"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="ShipmentMethodID"
            length="10"
        </meta>    
    </property>
    
    <property
        name="yesNo1"
        type="boolean"
        column="YesNo1"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="YesNo1"
        </meta>    
    </property>
    
    <property
        name="yesNo2"
        type="boolean"
        column="YesNo2"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="YesNo2"
        </meta>    
    </property>



    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to AgentAddress -->
    <many-to-one
        name="agentAddressByShipperId"
        class="edu.ku.brc.specify.datamodel.AgentAddress"
        not-null="true"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="ShipperID"         
        </meta>
        <column name="ShipperID" />
    </many-to-one>
    <!-- bi-directional many-to-one association to AgentAddress -->
    <many-to-one
        name="agentAddressByShippedToId"
        class="edu.ku.brc.specify.datamodel.AgentAddress"
        not-null="true"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="ShippedToID"         
        </meta>
        <column name="ShippedToID" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one
        name="agent"
        class="edu.ku.brc.specify.datamodel.Agent"
        not-null="true"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="ShippedByID"         
        </meta>
        <column name="ShippedByID" />
    </many-to-one>
	
	<!-- this was missing but existing in original mapping file - now replaced -->
    <!-- bi-directional one-to-many association to BorrowShipment -->
    <set
        name="borrowShipments"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="none"

           @hibernate.collection-key
            column="ShipmentID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.BorrowShipment"
        </meta>
        <key>
            <column name="ShipmentID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.BorrowShipment"
        />
    </set>
	
    <!-- bi-directional one-to-many association to Loan -->
    <set
        name="loans"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="none"

           @hibernate.collection-key
            column="ShipmentID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.Loan"
        </meta>
        <key>
            <column name="ShipmentID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.Loan"
        />
    </set>
    <!-- bi-directional one-to-many association to ExchangeOut -->
    <set
        name="exchangeOuts"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="none"

           @hibernate.collection-key
            column="ShipmentID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.ExchangeOut"
        </meta>
        <key>
            <column name="ShipmentID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.ExchangeOut"
        />
    </set>

</class>
</hibernate-mapping>
