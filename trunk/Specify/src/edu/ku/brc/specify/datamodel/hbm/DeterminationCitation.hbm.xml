<class name="edu.ku.brc.specify.datamodel.DeterminationCitation" table="determinationcitation" lazy="true">
    <meta attribute="class-description" inherit="false">
	Associates publications with Determinations.
	</meta>

    <id name="determinationCitationId" type="java.lang.Integer" column="DeterminationCitationID">
        <meta attribute="field-description"></meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampModified" type="java.sql.Timestamp" column="TimestampModified" length="23" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to ReferenceWork -->
    <many-to-one name="referenceWork" class="edu.ku.brc.specify.datamodel.ReferenceWork" not-null="true" outer-join="true" index="IX_DeterminationCitationReferenceWorkID" unique-key="IX_DeterminationCitation">
        <meta attribute="field-description">
	   ID of the publication citing the determination
	</meta>
        <column name="ReferenceWorkID"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Determination -->
    <many-to-one name="determination" class="edu.ku.brc.specify.datamodel.Determination" not-null="true" outer-join="true" cascade="none" index="IX_DeterminationCitationDeterminationID" unique-key="IX_DeterminationCitation">
        <meta attribute="field-description">
	   Determination being cited
	</meta>
        <column name="DeterminationID"/>
    </many-to-one>

</class>
</hibernate-mapping>