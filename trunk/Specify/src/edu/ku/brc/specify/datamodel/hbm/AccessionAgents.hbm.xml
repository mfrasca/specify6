<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.AccessionAgents" table="accessionagents" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="accessionAgentsId" type="java.lang.Long" column="AccessionAgentsID">
        <meta attribute="field-description"></meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>

    <timestamp name="timestampModified" column="TimestampModified">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Versioning TimeStamp</meta>
    </timestamp>

    <property name="role" type="java.lang.String" column="Role" not-null="true" length="32" unique-key="IX_AccessionAgents">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Role the agent played in the accession process</meta>
    </property>

    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <!-- Associations -->

    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one name="Agent" class="edu.ku.brc.specify.datamodel.Agent" not-null="true" outer-join="true" cascade="none" index="IX_AccessionAgentsAgentID" unique-key="IX_AccessionAgents">
        <meta attribute="field-description">AgentAdress of agent playing role in Accession</meta>
        <column name="AgentID"/>
    </many-to-one>

    <!-- bi-directional many-to-one association to Accession -->
    <many-to-one name="accession" class="edu.ku.brc.specify.datamodel.Accession" not-null="true" outer-join="true" cascade="none" index="IX_AccessionAgentsAccessionID" unique-key="IX_AccessionAgents">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Accession in which the Agent played a role</meta>
        <column name="AccessionID"/>
    </many-to-one>

    <!-- bi-directional many-to-one association to RepositoryAgreement -->
    <many-to-one name="repositoryAgreement" class="edu.ku.brc.specify.datamodel.RepositoryAgreement" not-null="true" outer-join="true" cascade="none">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <column name="RepositoryAgreementID"/>
    </many-to-one>

</class>
</hibernate-mapping>
