<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.Accession" table="accession" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="accessionId" type="java.lang.Long" column="AccessionID">
        <meta attribute="field-description">Primary key</meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>
    
    <timestamp name="timestampModified" column="TimestampModified">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Versioning TimeStamp</meta>
    </timestamp>
    
    <property name="type" type="java.lang.String" column="Type" length="32">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Source of Accession, e.g. 'Collecting', 'Gift',  'Bequest' ...</meta>
    </property>
    
    <property name="status" type="java.lang.String" column="Status" length="32">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Status of Accession, e.g. 'In process', 'Complete' ...</meta>
    </property>
    
    <property name="number" type="java.lang.String" column="Number" not-null="true" length="60" index="IX_Accession_3">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">A user-visible identifier of the Accession. Typically an integer, but may include alphanumeric characters as prefix, suffix, and separators</meta>
    </property>
    
    <property name="verbatimDate" type="java.lang.String" column="VerbatimDate" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">accomodates historical accessions.</meta>
    </property>
    
    <property name="dateAccessioned" type="calendar_date" column="DateAccessioned">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Date of Accession</meta>
    </property>
    
    <property name="dateReceived" type="calendar_date" column="DateReceived">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Date material was received</meta>
    </property>
    
    <property name="text1" type="text" column="Text1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="text2" type="text" column="Text2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="text3" type="text" column="Text3">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="number1" type="java.lang.Float" column="Number1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="number2" type="java.lang.Float" column="Number2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Comments</meta>
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50" index="IX_Accession_2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Login name of user who last edited the record</meta>
    </property>
    
    <property name="yesNo1" type="boolean" column="YesNo1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="yesNo2" type="boolean" column="YesNo2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>



    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to CollectionObject -->
    <set name="collectionObjects" lazy="true" inverse="true" cascade="none">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <key>
            <column name="AccessionID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.CollectionObject"/>
    </set>
    
    <!-- bi-directional one-to-many association to AccessionAuthorizations -->
    <set name="accessionAuthorizations" lazy="true" inverse="true" cascade="all-delete-orphan">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <key>
            <column name="AccessionID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.AccessionAuthorizations"/>
    </set>
    
    <!-- bi-directional one-to-many association to AccessionAgents -->
    <set name="accessionAgents" lazy="true" inverse="true" cascade="all-delete-orphan">
        <!-- meg change cascade="none" to cascade="delete - above and below -->
        <meta attribute="scope-field">protected</meta> 
        <meta attribute="field-description"></meta>
        <key>
            <column name="AccessionID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.AccessionAgents"/>
    </set>
    
</class>
</hibernate-mapping>
