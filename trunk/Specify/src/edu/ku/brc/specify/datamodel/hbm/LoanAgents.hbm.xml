<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.LoanAgents" table="loanagents" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="loanAgentsId" type="java.lang.Integer" column="LoanAgentsID">
        <meta attribute="field-description"></meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>

    <property name="role" type="java.lang.String" column="Role" not-null="true" length="32" unique-key="IX_LoanAgents">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Role the agent played in the loan</meta>
    </property>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampModified" type="java.sql.Timestamp" column="TimestampModified" length="23">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Loan -->
    <many-to-one name="loan" class="edu.ku.brc.specify.datamodel.Loan" not-null="true" outer-join="true" cascade="none" index="IX_LoanAgentsLoanID" unique-key="IX_LoanAgents">
        <meta attribute="field-description">ID of loan agent at AgentID played a role in</meta>
        <column name="LoanID"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one name="Agent" class="edu.ku.brc.specify.datamodel.Agent" not-null="true" outer-join="true" index="IX_LoanAgentsAgentID" unique-key="IX_LoanAgents">
        <meta attribute="field-description">Agent</meta>
        <column name="AgentID"/>
    </many-to-one>

</class>
</hibernate-mapping>
