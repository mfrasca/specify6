<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.RepositoryAgreement" table="repositoryAgreement" lazy="true">
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="repositoryAgreement"
    </meta>

    <id name="repositoryAgreementId" type="java.lang.Integer" column="RepositoryAgreementID">
        <meta attribute="field-description"></meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>

    <property name="number" type="java.lang.String" column="Number" not-null="true" length="60">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="status" type="java.lang.String" column="Status" length="32">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="startDate" type="calendar_date" column="StartDate">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="endDate" type="calendar_date" column="EndDate">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="dateReceived" type="calendar_date" column="DateReceived">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="text1" type="text" column="Text1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="text2" type="text" column="Text2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="text3" type="text" column="Text3">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="number1" type="java.lang.Float" column="Number1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="number2" type="java.lang.Float" column="Number2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="timestampModified" type="java.sql.Timestamp" column="TimestampModified" length="23">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="yesNo1" type="boolean" column="YesNo1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>

    <property name="yesNo2" type="boolean" column="YesNo2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </property>



    <!-- Associations -->

    <!-- bi-directional one-to-many association to CollectionObject -->
    <set name="collectionObjects" lazy="true" inverse="true" cascade="none">
	    <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <key>
            <column name="RepositoryAgreementID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.CollectionObject"/>
    </set>

    <!-- bi-directional one-to-many association to AccessionAuthorizations -->
    <set name="repositoryAgreementAuthorizations" lazy="true" inverse="true" cascade="all">
	    <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <key>
            <column name="RepositoryAgreementID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.AccessionAuthorizations"/>
    </set>

    <!-- bi-directional one-to-many association to AccessionAgent -->
    <set name="repositoryAgreementAgents" lazy="true" inverse="true" cascade="delete">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <key>
            <column name="RepositoryAgreementID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.AccessionAgent"/>
    </set>

    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one name="originator" class="edu.ku.brc.specify.datamodel.Agent" not-null="true" outer-join="true" cascade="none">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <column name="AgentID"/>
    </many-to-one>
    
</class>
</hibernate-mapping>