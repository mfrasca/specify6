<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.CollectingEvent" table="collectingevent" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="collectingEventId" type="java.lang.Long" column="CollectingEventID">
        <meta attribute="field-description">Primary key</meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>

    <timestamp name="timestampModified" column="TimestampModified">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Versioning TimeStamp</meta>
    </timestamp>

    <property name="stationFieldNumber" type="java.lang.String" column="StationFieldNumber" length="50" index="IX_CollectingEventStatFldNo">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Station number or field number of the site where collecting event took place, A number or code recorded in field notes and/or written on field tags that identifies ALL material collected in a CollectingEvent.</meta>
    </property>
    
    <property name="method" type="java.lang.String" column="Method" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">The method used to obtain the biological object</meta>
    </property>
    
    <property name="verbatimDate" type="java.lang.String" column="VerbatimDate" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Date which collector recorded in field book, exactly as reported by the collector.  Should indicate whether reported as range, season, month, etc.</meta>
    </property>
    
    <property name="startDate" type="calendar_date" column="StartDate">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">The date collecting event began</meta>
    </property>
    
    <property name="startDatePrecision" type="short" column="StartDatePrecision">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="startDateVerbatim" type="java.lang.String" column="StartDateVerbatim" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="endDate" type="calendar_date" column="EndDate">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">The date collecting event ended</meta>
    </property>
    
    <property name="endDatePrecision" type="short" column="EndDatePrecision">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="endDateVerbatim" type="java.lang.String" column="EndDateVerbatim" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="startTime" type="short" column="StartTime">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Start time in military format</meta>
    </property>
    
    <property name="endTime" type="short" column="EndTime">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">End time in military format</meta>
    </property>
    
    <property name="verbatimLocality" type="text" column="VerbatimLocality">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Original statement (literal quotation) of the location of the CollectingEvent as given by the Collector.</meta>
    </property>
    
    <property name="groupPermittedToView" type="java.lang.Integer" column="GroupPermittedToView" length="10" index="IX_CollectingEventGPV">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">The name of the group that this record is visible to.</meta>
    </property>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Free text to record information that does not conform to structured fields, or to explain data recorded in those fields, particularly problematic interpretations of data given by collector(s).</meta>
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50" index="IX_CollectingEvent_2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

    <!-- Associations -->

    <!-- bi-directional one-to-many association to CollectionObject -->
    <set name="collectionObjects" lazy="true" inverse="true" cascade="none">    
    <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta> 
        <key>
            <column name="CollectingEventID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.CollectionObject"/>
    </set>
    
    <!-- bi-directional one-to-many association to Collectors -->
    <set name="collectors" lazy="true" inverse="true" cascade="delete">
        <meta attribute="field-description"></meta>
        <key>
            <column name="CollectingEventID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.Collectors"/>
    </set>
    
   <!-- bi-directional many-to-one association to Locality -->
   <many-to-one name="locality" class="edu.ku.brc.specify.datamodel.Locality" not-null="true" outer-join="true" index="IX_CollectingEventLocalityID">
    <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Locality where collection took place</meta>
        <column name="LocalityID"/>
    </many-to-one>
    
    <!-- bi-directional one-to-one association to Stratigraphy -->
    <one-to-one name="stratigraphy" class="edu.ku.brc.specify.datamodel.Stratigraphy" cascade="save-update">
    <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
    </one-to-one>
    
    <!-- bi-directional one-to-many association to CollectingEventAttr -->
    <set name="attrs" lazy="true" inverse="true" cascade="delete">
    <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <key>
            <column name="CollectingEventAttrID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.CollectingEventAttr"/>
    </set>    
        
    <!-- bi-directional many-to-many association to ExternalResource -->
    <set name="externalResources" table="coleve_extres" lazy="true" inverse="true" cascade="save-update">
        <key>
            <column name="CollectingEventID"/>
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.ExternalResource" column="ExternalResourceID"/>
    </set>
    
</class>
</hibernate-mapping>
