<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="edu.ku.brc.specify.datamodel.LoanAgent" 
    table="loanagents"
	lazy="true"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="loanagents"
    </meta>

    <id
        name="loanAgentsId"
        type="java.lang.Integer"
        column="LoanAgentsID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="native"
            type="java.lang.Integer"
            column="LoanAgentsID"

        </meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native" />
    </id>

    <property
        name="role"
        type="java.lang.String"
        column="Role"
        not-null="true"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Role"
            length="50"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="remarks"
        type="text"
        column="Remarks"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
        </meta>    
    </property>
    
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
        not-null="true"
        update="false"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
            update="false"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="lastEditedBy"
        type="java.lang.String"
        column="LastEditedBy"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="LastEditedBy"
            length="50"
        </meta>    
    </property>
    <property
        name="roleId"
        type="java.lang.Integer"
        column="RoleID"
        length="10"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="RoleID"
            length="10"
        </meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Loan -->
    <many-to-one
        name="loan"
        class="edu.ku.brc.specify.datamodel.Loan"
        not-null="true"
		outer-join="true"
		cascade="none"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
			cascade="none"
           @hibernate.column name="LoanID"         
        </meta>
        <column name="LoanID" />
    </many-to-one>
    
    <!-- bi-directional many-to-one association to AgentAddress -->
    <many-to-one
        name="agentAddress"
        class="edu.ku.brc.specify.datamodel.AgentAddress"
        not-null="true"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="AgentAddressID"         
        </meta>
        <column name="AgentAddressID" />
    </many-to-one>

</class>
</hibernate-mapping>
