            lazy="true"
            inverse="true"
            cascade="none"

           @hibernate.collection-key
            column="TaxonomyTypeID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.CollectionTaxonomyType"
        </meta>
        <key>
            <column name="TaxonomyTypeID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.CollectionTaxonomyType"
        />
    </set>

</class>
