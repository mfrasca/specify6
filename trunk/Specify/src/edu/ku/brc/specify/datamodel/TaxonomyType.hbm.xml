    table="taxonomytype"
lazy="true"	
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="taxonomytype"
    </meta>

    <id
        name="taxonomyTypeId"
        type="java.lang.Integer"
        column="TaxonomyTypeID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Integer"
            column="TaxonomyTypeID"

        </meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="assigned" />
    </id>

    <property
        name="kingdomId"
        type="java.lang.Integer"
        column="KingdomID"
        length="10"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="KingdomID"
            length="10"
        </meta>    
    </property>
    <property
        name="taxonomyTypeName"
        type="java.lang.String"
        column="TaxonomyTypeName"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TaxonomyTypeName"
            length="50"
        </meta>    
    </property>
    <property
        name="treeInfoUpToDate"
        type="boolean"
        column="TreeInfoUpToDate"
        not-null="true"
        length="1"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TreeInfoUpToDate"
            length="1"
            not-null="true"
        </meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to TaxonName -->
    <set
        name="taxonNames"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="none"

           @hibernate.collection-key
            column="TaxonomyTypeID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.TaxonName"
        </meta>
        <key>
            <column name="TaxonomyTypeID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.TaxonName"
        />
    </set>
    <!-- bi-directional one-to-many association to TaxonomicUnitType -->
    <set
        name="taxonomicUnitTypes"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="none"

           @hibernate.collection-key
            column="TaxonomyTypeID"

           @hibernate.collection-one-to-many
