<?xml version="1.0" encoding="UTF-8"?>
<!-- tableid means it is making it's service available for any tables with 
     that ID --> 
<plugins>
	<core>
	    <plugin class="edu.ku.brc.specify.tasks.StartUpTask"/>
	    <plugin class="edu.ku.brc.specify.tasks.DataEntryTask">
	    	<service command="Edit" tableid="1" tooltip="EditRecordSetTT"/>
	    	<service command="Edit" tableid="7" tooltip="EditRecordSetTT"/>
	    </plugin>
	    <plugin class="edu.ku.brc.specify.tasks.LabelsTask">
	    	<service command="DoLabels" tableid="1" tooltip="CreateLabelTT"/>
	        <service command="DoLabels" tableid="7" tooltip="CreateLabelTT"/>
	    </plugin>
	    <plugin class="edu.ku.brc.specify.tasks.ReportsTask"/>
	    <plugin class="edu.ku.brc.specify.tasks.LocalityMapperTask">
	        <service command="DoLocalityMap" tableid="10" tooltip="CreateLocalityMapTT"/>
	        <service command="DoLocalityMap" tableid="11" tooltip="CreateLocalityMapTT"/>
	    </plugin>
	    <plugin class="edu.ku.brc.specify.tasks.InteractionsTask"/>
	    <plugin class="edu.ku.brc.specify.tasks.StatsTask"/>
	    <plugin class="edu.ku.brc.specify.tasks.QueryTask"/>
        <plugin class="edu.ku.brc.specify.tasks.RecordSetTask">
	    	<service command="Save" tableid="1" tooltip="CreateRecordSetTT"/>
            <service command="Save" tableid="7" tooltip="CreateRecordSetTT"/>
        </plugin>
        <plugin class="edu.ku.brc.specify.tasks.InfoRequestTask">
	    	<service command="Save" tableid="1" tooltip="CreateInfoRequestTT"/>
	    </plugin>
	    <plugin class="edu.ku.brc.specify.tasks.SystemSetupTask"/>
		<plugin class="edu.ku.brc.specify.tasks.WorkbenchTask"/>
	    <plugin class="edu.ku.brc.specify.tasks.ExpressSearchTask"/>

	</core>
	<external/>
</plugins>
