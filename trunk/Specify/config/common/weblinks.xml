<vector>
  <weblinkdef>
    <name>MailTo</name>
    <desc>Launches an email message window for sending an email.</desc>
    <baseURLStr>mailto:[email]</baseURLStr>
    <args>
      <weblinkdefarg>
        <name>email</name>
        <title>EMail</title>
        <prompt>true</prompt>
      </weblinkdefarg>
    </args>
    <usedByList>
      <usedby>
        <tableName>agent</tableName>
        <fieldName>email</fieldName>
      </usedby>
    </usedByList>
  </weblinkdef>
  <weblinkdef>
    <name>FishBase</name>
    <desc>Displays a FishBase page for the Genus and Species</desc>
    <baseURLStr>http://www.fishbase.org/Summary/speciesSummary.php?genusname=[genus]AMPspeciesname=[species]</baseURLStr>
    <args>
      <weblinkdefarg>
        <name>genus</name>
        <title>Genus</title>
        <prompt>false</prompt>
      </weblinkdefarg>
      <weblinkdefarg>
        <name>species</name>
        <title>Species</title>
        <prompt>false</prompt>
      </weblinkdefarg>
    </args>
    <usedByList>
    </usedByList>
  </weblinkdef>
</vector>