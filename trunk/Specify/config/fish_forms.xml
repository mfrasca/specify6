<?xml version="1.0" encoding="UTF-8"?>
<viewset name="Fish Views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="form.xsd">

    <view id="1" type="form" 
                 name="CollectionObject" 
                 class="edu.ku.brc.specify.datamodel.CollectionObject" 
                 gettable="edu.ku.brc.specify.ui.forms.DataGetterForObj"
                 resourceLabels="false" 
                 validate="true"
                 >
    	<desc><![CDATA[The Collection Object form.]]></desc>
    	
		<columnDef>100px,3dlu,p,3dlu,80px,3dlu,p,3dlu,60px,3dlu,p,p:g</columnDef>
		<rowDef>p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,10dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p</rowDef>

		<rows>
			<row>
				<cell type="separator" label="KU Fish" colspan="12"/>
			</row>
			<row>
			    <cell type="label" labelfor="catalogNumber" label="Catalog Number"/>
				<cell type="field" name="catalogNumber" format="%.0f"/>
				
                <cell type="label" labelfor="catalogSeries.seriesName" label="Catalog Series"/>
				<cell type="field" uitype="label" name="catalogSeries.seriesName"/>
			</row>
			<row>
                <cell type="label" labelfor="accession.number" label="Accession #"/>
				<cell type="field" name="accession.number"/>
				
                <cell type="label" labelfor="agent.lastName,agent.firstName" label="Cataloger"/>
				<cell type="field" name="agent.lastName,agent.firstName" format="%s,%s"/>
				
                <cell type="label" labelfor="catalogedDate" label="Date"/>
				<cell type="field" name="catalogedDate" format="date"/>
			</row>
			<row>
				<cell type="separator" label="Determination" colspan="12"/>
			</row>

			<row>
				<cell type="subview" id="6" name="determinations" class="edu.ku.brc.specify.datamodel.Determination"  colspan="12"/>
			</row>

			<row>
				<cell type="separator" label="Locality" colspan="12"/>
			</row>
			<row>
                <cell type="label" labelfor="fieldNumber" label="Field No"/>
				<cell type="field" name="fieldNumber"/>
				
                <cell type="label" labelfor="collectingEvent.locality.localityName" label="Locality"/>
				<cell type="field" name="collectingEvent.locality.localityName" cols="41" colspan="6"/>
			</row>
			
			<row>
				<cell type="separator" label="Preparations" colspan="12"/>
			</row>
			<row>
				<cell type="subview" id="7" name="preparations" class="edu.ku.brc.specify.datamodel.Preparations" colspan="12"/>
			</row>

			<row>
				<cell type="separator" label="" colspan="12"/>
			</row>
			<row>
                <cell type="label" labelfor="lastEditedBy" label="Edited By"/>
				<cell type="field" name="lastEditedBy"/>
				
                <cell type="label" labelfor="timestampModified" label="Last Edited"/>
				<cell type="field" name="timestampModified" cols="20" colspan="2"/>
			</row>
		</rows>
	</view>
	
	<view id="6" type="form" 
                 name="Determination" 
                 class="edu.ku.brc.specify.datamodel.Determination" 
                 gettable="edu.ku.brc.specify.ui.forms.DataGetterForObj"
                 resourceLabels="false" 
                 validate="true"
                 >
	    <altviews>
			<alt id="6" label="Form"/>
			<!-- <alt id="66" label="Table"/> -->
		</altviews>
		
		<columnDef>100px,3dlu,p,3dlu,80px,3dlu,p,3dlu,60px,3dlu,p,p:g</columnDef>
		<rowDef>p,2dlu,p,2dlu,p,2dlu,p:g,2dlu</rowDef>
	  
		<rows>
			<row>
                <cell type="label" labelfor="fieldNumber" label="Taxon"/>
				<cell type="field" name="taxon.fullTaxon" label="Taxon" colspan="5"/>	
				
                <cell type="label" labelfor="isCurrent" label=""/>
				<cell type="field" name="isCurrent" uitype="checkbox" label="Is Current"/>
			</row>
			<row>
				
                <cell type="label" labelfor="agent.lastName,agent.firstName" label="Determiner"/>
				<cell type="field" name="agent.lastName,agent.firstName" format="%s, %s" cols="20"/>
				
                <cell type="label" labelfor="confidenceId" label="Confidence"/>
				<cell type="field" name="confidenceId" label="Confidence"/>
				
                <cell type="label" labelfor="determinationDate" label="Date"/>
				<cell type="field" name="determinedDate" isrequired="true"/>
			</row>
			
			<row>
                <cell type="label" labelfor="remarks" label="Remarks"/>
				<cell type="field" name="remarks" colspan="9" cols="50"/>	
			</row>
		</rows>
	</view>
	
