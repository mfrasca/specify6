                </row>
                <row>
                    <cell type="label" labelfor="3" label="Last Name"/>
                    <cell type="field" id="3" name="lastName" uitype="text" cols="20"  isrequired="true" valtype="Changed"/>
                </row>
                <row>
                    <cell type="label" labelfor="4" label="Middle Initial"/>
                    <cell type="field" id="4" name="middleInitial" uitype="text" cols="3" valtype="Changed"/>
                </row>
                <row>
                    <cell type="label" labelfor="5" label="First Name"/>
                    <cell type="field" id="5" name="firstName" uitype="text" cols="20" isrequired="true" valtype="Changed"/>
                </row>
                <row>
                    <cell type="label" labelfor="7" label="E-mail"/>
                    <cell type="field" id="7" name="email" uitype="text" cols="32" valtype="Changed"/>
                </row>
                <row>
                    <cell type="label" labelfor="8" label="Abbreviation"/>
                    <cell type="field" id="8" name="abbreviation" uitype="text" cols="20" valtype="Changed"/>
                </row>
                <row>
                    <cell type="subview" viewname="Address" id="9" name="addresses" colspan="6" valtype="Changed"/>
                </row>
			</rows>
        </viewdef>
        
        <viewdef
            type="form"
            name="Organization"
            class="edu.ku.brc.specify.datamodel.Agent"
            gettable="edu.ku.brc.ui.forms.DataGetterForObj"
