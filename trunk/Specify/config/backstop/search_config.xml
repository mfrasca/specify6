        <table id="113" tableid="52" name="OpenLoans" title="Open Loans" priority="1" color="30, 144, 255">
            <detailView icon="Loan">
                <sql>
                    <![CDATA[SELECT DISTINCT loan.LoanID, LoanNumber, LoanDate, CurrentDueDate, OriginalDueDate FROM loan INNER JOIN loanphysicalobject ON loan.LoanID = loanphysicalobject.LoanID
 INNER JOIN preparation ON loanphysicalobject.PreparationID = preparation.PreparationID
 INNER JOIN collectionobject ON preparation.CollectionObjectID = collectionobject.CollectionObjectID
 INNER JOIN collection ON collectionobject.CollectingEventID = collection.CollectionID
 INNER JOIN collectiontype ON collection.CollectionTypeID = collectiontype.CollectionTypeID
 INNER JOIN specifyuser ON collectiontype.SpecifyUserID = specifyuser.SpecifyUserID WHERE specifyuser.SpecifyUserID = SPECIFYUSERID AND
 (not (currentduedate is null)) AND loan.IsGift = false AND (IsClosed = false or IsClosed is null) AND 
                    datediff(CURDATE(), currentduedate) > 0]]>
                </sql>
                <captions>
                    <caption col="loan.LoanID" text="LoanID" visible="false"/>
                    <caption col="LoanNumber" text="Number"/>
                    <caption col="LoanDate" text="Loan Date"/>
                    <caption col="CurrentDueDate" text="Due Date"/>
                    <caption col="OriginalDueDate" text="Orig Due Date"/>
                </captions>
                <recordset tabletype="52" col="1"/>
            </detailView>
        </table>
        
