  <table id="1" usehitscache="true" name="CollObject" title="Collection Object" priority="1" color="30, 144, 255" expresssearch="true">
    <outofdate>
        <table name="collectionobject" title="Collection Object"/>
        <table name="determination" title="Determination"/>
        <table name="taxon" title="Taxon"/>
    </outofdate>
    <index>
	    <sql>
	        <![CDATA[Select ColFieldsDef From collectionobject Inner Join determination ON determination.CollectionObjectID = collectionobject.CollectionObjectID Inner Join taxon ON taxon.TaxonID = determination.TaxonID]]>
	    </sql>
	    <cols>
	       <col pos="1" id="true">collectionobject.CollectionObjectID</col>
	       <col pos="2">collectionobject.CatalogNumber</col>
	       <col pos="3">collectionobject.CatalogedDate</col>
	       <col pos="4">taxon.FullName</col>
	       <join>determination.CollectionObjectID</join>
	       <join>determination.TaxonID</join>
	    </cols>
	</index>
    <detailView icon="ColObj">
	    <sql>
	        <![CDATA[Select CollectionObjectID from collectionobject where CollectionObjectID in (%s)]]>
	    </sql>
        <captions>
            <caption col="CollectionObjectID" text="CollectionObjectID" visible="false"/>
            <caption col="CatalogNumber" text="Catalog Number"/>
            <caption col="CatalogedDate" text="Cataloged Date"/>
            <caption col="FullName" text="Taxon Name"/>
        </captions>
        <recordset tabletype="1" col="1"/>
	</detailView>
  </table>

