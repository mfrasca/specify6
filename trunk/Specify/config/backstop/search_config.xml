        <table id="9" tableid="9" usehitscache="true" name="CollObjToDeterminer" title="Determiner" priority="1" color="30, 144, 255">
            <outofdate>
                <table name="determination" title="Determination"/>
            </outofdate>
            <index>
                <sql>
                    <![CDATA[SELECT ColFieldsDef FROM
    collectionobject
    INNER JOIN determination ON determination.CollectionObjectID = collectionobject.CollectionObjectID
    INNER JOIN agent ON determination.DeterminerID = agent.AgentID
    INNER JOIN taxon ON determination.TaxonID = taxon.TaxonID 
    INNER JOIN determinationstatus ON determination.DeterminationStatusID = determinationstatus.DeterminationStatusID 
    WHERE determinationstatus.DeterminationStatusID = 2]]>
                </sql>
                <cols>
                    <col pos="1" id="true">collectionobject.CollectionObjectID</col>
                    <col pos="2">taxon.FullName</col>
                    <col pos="3">taxon.CommonName</col>
                    <col pos="4">agent.LastName</col>
                    <col pos="5">agent.FirstName</col>
                </cols>
            </index>
            <detailView icon="Agent">
                <sql>
                    <![CDATA[SELECT * FROM (SELECT
taxon.FullName,
taxon.CommonName,
agent.LastName,
agent.FirstName,
collectionobject.CollectionObjectID,
FROM
collectionobject
INNER JOIN determination ON determination.CollectionObjectID = collectionobject.CollectionObjectID
INNER JOIN agent ON determination.DeterminerID = agent.AgentID
INNER JOIN taxon ON determination.TaxonID = taxon.TaxonID 
INNER JOIN determinationstatus ON determination.DeterminationStatusID = determinationstatus.DeterminationStatusID 
WHERE determinationstatus.DeterminationStatusID = 2) AS tx
WHERE tx.CollectionObjectID IN (%s)]]>
                </sql>
                <captions>
                    <caption col="CollectionObjectCatalogID" text="ID" visible="false"/>
                    <caption col="TaxonName" text="Taxon Name"/>
                    <caption col="CommonName" text="Common Name"/>
                    <caption col="LastName" text="Last Name"/>
                    <caption col="FirstName" text="First Name"/>
               </captions>
               <recordset tabletype="7" col="1"/>
            </detailView>
        </table>
    
