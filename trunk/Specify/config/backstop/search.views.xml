                </row>
                <row>
                    <cell type="label" labelfor="2" label="Discipline"/>
                    <cell type="field" id="2" name="Discipline" isrequired="true" uitype="text"/>
                </row>
            </rows>
        </viewdef>
        
        <viewdef type="form"
            name="CatalogNumberingSchemeSearch"
            class="edu.ku.brc.specify.datamodel.CatalogNumberingScheme"
            gettable="edu.ku.brc.ui.forms.DataGetterForHashMap"
            settable="edu.ku.brc.ui.forms.DataSetterForHashMap">
            
            <desc><![CDATA[The CatalogNumberingScheme Search Form]]></desc>
