                </row>
                <row>
                    <cell type="label" labelfor="2" label="Is Numeric"/>
                    <cell type="field" id="2" name="IsNumericOnly" isrequired="true" uitype="text"/>
                </row>
            </rows>
        </viewdef>
        
    </viewdefs>
