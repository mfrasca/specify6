<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="edu.ku.brc.specify.datamodel.Project" 
    table="project"
	lazy="true"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="project"
    </meta>

    <id
        name="projectId"
        type="java.lang.Integer"
        column="ProjectID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Integer"
            column="ProjectID"

        </meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="assigned" />
    </id>

    <property
        name="projectName"
        type="java.lang.String"
        column="ProjectName"
        not-null="true"
        length="50"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="ProjectName"
            length="50"
            not-null="true"
        </meta>    
    </property>
    <property
        name="projectDescription"
        type="text"
        column="ProjectDescription"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="ProjectDescription"
        </meta>    
    </property>
    <property
        name="url"
        type="java.lang.String"
        column="URL"
        length="300"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="URL"
            length="300"
        </meta>    
    </property>
    <property
        name="startDate"
        type="calendar_date"
        column="StartDate"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="StartDate"
        </meta>    
    </property>
    <property
        name="endDate"
        type="calendar_date"
        column="EndDate"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="EndDate"
        </meta>    
    </property>
    
    <property
        name="remarks"
        type="text"
        column="Remarks"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
        </meta>    
    </property>
    
    <property
        name="text1"
        type="java.lang.String"
        column="Text1"
        length="300"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="Text1"
            length="300"
        </meta>    
    </property>
    <property
        name="text2"
        type="java.lang.String"
        column="Text2"
        length="300"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="Text2"
            length="300"
        </meta>    
    </property>
    <property
        name="number1"
        type="java.lang.Float"
        column="Number1"
        length="24"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="Number1"
            length="24"
        </meta>    
    </property>
    <property
        name="number2"
        type="java.lang.Float"
        column="Number2"
        length="24"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="Number2"
            length="24"
        </meta>    
    </property>
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
        </meta>    
    </property>
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
        </meta>    
    </property>
    <property
        name="lastEditedBy"
        type="java.lang.String"
        column="LastEditedBy"
        length="50"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="LastEditedBy"
            length="50"
        </meta>    
    </property>
    
    <property
        name="yesNo1"
        type="boolean"
        column="YesNo1"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="YesNo1"
        </meta>    
    </property>
    
    <property
        name="yesNo2"
        type="boolean"
        column="YesNo2"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="YesNo2"
        </meta>    
    </property>



    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one
        name="agent"
        class="edu.ku.brc.specify.datamodel.Agent"
        not-null="true"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="ProjectAgentID"         
        </meta>
        <column name="ProjectAgentID" />
    </many-to-one>
    <!-- bi-directional one-to-many association to ProjectCollectionObject -->
    <set
        name="projectCollectionObjects"
        lazy="true"
        inverse="true"
		cascade="delete"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="delete"

           @hibernate.collection-key
            column="ProjectID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.ProjectCollectionObject"
        </meta>
        <key>
            <column name="ProjectID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.ProjectCollectionObject"
        />
    </set>

</class>
</hibernate-mapping>
