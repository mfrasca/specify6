<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="edu.ku.brc.specify.datamodel.CatalogSeries" 
    table="catalogseries"
	lazy="true"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="catalogseries"
    </meta>

    <id
        name="catalogSeriesId"
        type="java.lang.Integer"
        column="CatalogSeriesID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Integer"
            column="CatalogSeriesID"

        </meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="assigned" />
    </id>

    <property
        name="isTissueSeries"
        type="boolean"
        column="IsTissueSeries"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="IsTissueSeries"
        </meta>    
    </property>
    
    <property
        name="seriesName"
        type="java.lang.String"
        column="SeriesName"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="SeriesName"
            length="50"
        </meta>    
    </property>
    
    <property
        name="catalogSeriesPrefix"
        type="java.lang.String"
        column="CatalogSeriesPrefix"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="CatalogSeriesPrefix"
            length="50"
        </meta>    
    </property>
    
    <property
        name="remarks"
        type="text"
        column="Remarks"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
        </meta>    
    </property>
    
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
        not-null="true"
        update="false"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
            update="false"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="lastEditedBy"
        type="java.lang.String"
        column="LastEditedBy"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="LastEditedBy"
            length="50"
        </meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-many association to CollectionObjDef -->
    <set
        name="collectionObjDefItems"
        table="catseries_colobjdef"
        lazy="true"
		cascade="save-update">
        <key>
            <column name="CatalogSeriesID" />
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.CollectionObjDef" column="CollectionObjDefID"/>
    </set>
    
    <!-- bi-directional many-to-one association to CatalogSeries -->
    <many-to-one
        name="tissue"
        class="edu.ku.brc.specify.datamodel.CatalogSeries"
        cascade="save-update"
        not-null="false"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="TissueID"         
        </meta>
        <column name="TissueID" />
    </many-to-one>
    <!--
    <set name="vouchers" table="catalogseries" inverse="true">
		<meta attribute="scope-field">protected</meta>
		<key column="TissueID"/>
		<one-to-many class="edu.ku.brc.specify.datamodel.CatalogSeries"/>
    </set>
    -->
</class>
</hibernate-mapping>
