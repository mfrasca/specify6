<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="edu.ku.brc.specify.datamodel.AgentExternalFile"
    table="agentexternalfile"
	lazy="true"
>

    <meta attribute="implements">ExternalFile</meta>
    <id
        name="agentExternalFileId"
        type="java.lang.Integer"
        column="AgentExternalFileID"
    >
      <meta attribute="scope-set">public</meta>
      <meta attribute="scope-field">protected</meta>
      <generator class="native"/>
    </id>

    <property
        name="mimeType"
        type="java.lang.String"
        column="mimeType"
        length="32"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="mimeType"
            length="32"
        </meta>    
    </property>
    
    <property
        name="fileName"
        type="java.lang.String"
        column="FileName"
        length="128"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="FileName"
            length="128"
        </meta>    
    </property>
    
    <property
        name="fileCreatedDate"
        type="java.lang.Integer"
        column="fileCreatedDate"
        length="10"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="fileCreatedDate"
            length="10"
        </meta>    
    </property>
    
    <property
        name="remarks"
        type="java.lang.String"
        column="Remarks"
        length="1073741823"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
            length="1073741823"
        </meta>    
    </property>   
     
    <property
        name="externalLocation"
        type="java.lang.String"
        column="ExternalLocation"
        length="1024"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="ExternalLocation"
            length="1024"
        </meta>    
    </property>
    
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
        </meta>    
    </property>
    
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
        </meta>    
    </property>
    
    <property
        name="lastEditedBy"
        type="java.lang.String"
        column="LastEditedBy"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="LastEditedBy"
            length="50"
        </meta>    
    </property>

    <!-- Associations -->
    
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one
        name="createdByAgent"
        class="edu.ku.brc.specify.datamodel.Agent"
        not-null="true"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="MadeByID"         
        </meta>
        <column name="MadeByID" />
    </many-to-one>
    
    <!-- bi-directional many-to-many association to Agent -->
    <set
        name="externalFiles"
        table="agent_extfiles"
        lazy="true"
		cascade="save-update">
        <key>
            <column name="AgentExternalFileID" />
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.Agent" column="AgentID"/>
    </set>
    
</class>
</hibernate-mapping>
