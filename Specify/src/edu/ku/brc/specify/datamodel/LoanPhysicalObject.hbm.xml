           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="PhysicalObjectID"         
        </meta>
        <column name="PhysicalObjectID" />
    </many-to-one>
	
    <!-- bi-directional many-to-one association to Loan -->
