<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="edu.ku.brc.specify.datamodel.ExternalResource"
    table="externalresource"
	lazy="true"
>

   <id
        name="externalFileId"
        type="java.lang.Integer"
        column="ExternalResourceID"
    >
      <meta attribute="scope-set">public</meta>
      <meta attribute="scope-field">protected</meta>
      <generator class="native"/>
    </id>

    <property
        name="mimeType"
        type="java.lang.String"
        column="MimeType"
        length="32"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="MimeType"
            length="32"
        </meta>    
    </property>
    
    <property
        name="fileName"
        type="java.lang.String"
        column="FileName"
        length="128"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="FileName"
            length="128"
        </meta>    
    </property>
    
    <property
        name="fileCreatedDate"
        type="calendar_date"
        column="FileCreatedDate"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="FileCreatedDate"
        </meta>    
    </property>
    
    <property
        name="remarks"
        type="text"
        column="Remarks"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
        </meta>    
    </property>
    
    <property
        name="externalLocation"
        type="java.lang.String"
        column="ExternalLocation"
        length="1024"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="ExternalLocation"
            length="1024"
        </meta>    
    </property>
    
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
        </meta>    
    </property>
    
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
        </meta>    
    </property>
    
    <property
        name="lastEditedBy"
        type="java.lang.String"
        column="LastEditedBy"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="LastEditedBy"
            length="50"
        </meta>    
    </property>

    <!-- Associations -->
    
    <!-- bi-directional one-to-many association to ExtResAttrs -->
    <set
        name="extResAttrs"
        lazy="true"
        inverse="true"
		cascade="delete"
    >
	<meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="delete"

           @hibernate.collection-key
            column="ExtResAttrsID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.ExtResAttrs"
        </meta>
        <key>
            <column name="ExtResAttrsID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.ExtResAttrs"
        />
    </set>	
    
    
    
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one
        name="createdByAgent"
        class="edu.ku.brc.specify.datamodel.Agent"
        not-null="true"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="MadeByID"         
        </meta>
        <column name="MadeByID" />
    </many-to-one>
    
    <!-- bi-directional many-to-many association to Agent -->
    <set
        name="agents"
        table="agent_extres"
        lazy="true"
		cascade="save-update">
        <key>
            <column name="ExternalResourceID" />
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.Agent" column="AgentID"/>
    </set>
    
    <!-- bi-directional many-to-many association to CollectionObject -->
    <set
        name="collectionObjects"
        table="colobj_extres"
        lazy="true"
		cascade="save-update">
        <key>
            <column name="ExternalResourceID" />
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.CollectionObject" column="CollectionObjectID"/>
    </set>    
    
    <!-- bi-directional many-to-many association to CollectingEvent -->
    <set
        name="collectinEvents"
        table="coleve_extres"
        lazy="true"
		cascade="save-update">
        <key>
            <column name="ExternalResourceID" />
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.CollectingEvent" column="CollectingEventID"/>
    </set>    
    
    <!-- bi-directional many-to-many association to Loan -->
    <set
        name="loans"
        table="loan_extres"
        lazy="true"
		cascade="save-update">
        <key>
            <column name="ExternalResourceID" />
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.Loan" column="LoanID"/>
    </set>
    
    <!-- bi-directional many-to-many association to Locality -->
    <set
        name="localities"
        table="locality_extres"
        lazy="true"
		cascade="save-update">
        <key>
            <column name="ExternalResourceID" />
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.Locality" column="LocalityID"/>
    </set>
    
    <!-- bi-directional many-to-many association to Permit -->
    <set
        name="permits"
        table="permit_extres"
        lazy="true"
		cascade="save-update">
        <key>
            <column name="ExternalResourceID" />
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.Permit" column="PermitID"/>
    </set>    
    
    <!-- bi-directional many-to-many association to Preparation -->
    <set
        name="preparations"
        table="prep_extres"
        lazy="true"
		cascade="save-update">
        <key>
            <column name="ExternalResourceID" />
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.Preparation" column="PreparationID"/>
    </set>
        
    <!-- bi-directional many-to-many association to Taxon -->
    <set
        name="taxonomy"
        table="taxon_extres"
        lazy="true"
		cascade="save-update">
        <key>
            <column name="ExternalResourceID" />
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.Taxon" column="TaxonID"/>
    </set>
    
    </class>
</hibernate-mapping>
