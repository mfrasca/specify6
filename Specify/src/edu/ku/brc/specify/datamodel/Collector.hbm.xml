<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="edu.ku.brc.specify.datamodel.Collector" 
    table="collectors"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="collectors"
    </meta>

    <id
        name="collectorsId"
        type="java.lang.Integer"
        column="CollectorsID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Integer"
            column="CollectorsID"

        </meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="assigned" />
    </id>

    <property
        name="orderNumber"
        type="int"
        column="OrderNumber"
        not-null="true"
        length="10"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="OrderNumber"
            length="10"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="remarks"
        type="text"
        column="Remarks"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
        </meta>    
    </property>
    
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
        not-null="true"
        update="false"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
            update="false"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="lastEditedBy"
        type="java.lang.String"
        column="LastEditedBy"
        length="50"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="LastEditedBy"
            length="50"
        </meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to CollectingEvent -->
    <many-to-one
        name="collectingEvent"
        class="edu.ku.brc.specify.datamodel.CollectingEvent"
        not-null="true"
		cascade="delete"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
			cascade="delete"
           @hibernate.column name="CollectingEventID"         
        </meta>
        <column name="CollectingEventID" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one
        name="agent"
        class="edu.ku.brc.specify.datamodel.Agent"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="AgentID"         
        </meta>
        <column name="AgentID" />
    </many-to-one>

</class>
</hibernate-mapping>
