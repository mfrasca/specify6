<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

    <class name="edu.ku.brc.specify.datamodel.TaxonTreeDef" table="taxontreedef" lazy="true">
    <meta attribute="class-description" inherit="false"/>
        <meta attribute="implements">TreeDefinitionIface</meta>

        <id name="treeDefId" type="java.lang.Integer" column="TreeDefID">
            <meta attribute="scope-field">protected</meta>
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="field-description"></meta>
            <generator class="foreign">
                <param name="property">collObjDef</param>
            </generator>
        </id>

        <property name="name" type="java.lang.String" column="Name" length="64">
            <meta attribute="scope-field">protected</meta>
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="field-description"></meta>
        </property>

        <property name="remarks" type="java.lang.String" column="Remarks" length="255">
            <meta attribute="scope-field">protected</meta>
            <meta attribute="field-description"></meta>
        </property>
        
        <!-- Associations -->
        
        <one-to-one name="collObjDef" class="edu.ku.brc.specify.datamodel.CollectionObjDef" constrained="true"/>
        
        <set name="treeEntries" table="Taxon" inverse="true" lazy="true">
            <meta attribute="field-description"></meta>
            <key column="TreeDefID"/>
            <one-to-many class="edu.ku.brc.specify.datamodel.Taxon"/>
        </set>
        
        <set name="treeDefItems" table="TaxonTreeDefItem" inverse="true" lazy="false" cascade="save-update">
            <meta attribute="field-description"></meta>
            <key column="TreeDefID"/>
            <one-to-many class="edu.ku.brc.specify.datamodel.TaxonTreeDefItem"/>
        </set>

    </class>
    
</hibernate-mapping>
