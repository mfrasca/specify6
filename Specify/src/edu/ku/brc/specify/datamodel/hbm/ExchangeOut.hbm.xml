<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.ExchangeOut" table="exchangeout" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="exchangeOutId" type="java.lang.Long" column="ExchangeOutID">
        <meta attribute="field-description">Primary key</meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>

    <timestamp name="timestampModified" column="TimestampModified">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Versioning Timestamp</meta>
    </timestamp>

    <property name="exchangeDate" type="calendar_date" column="ExchangeDate">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Date exchange was sent</meta>
    </property>
    <property name="quantityExchanged" type="short" column="QuantityExchanged" not-null="true">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Number of items sent</meta>
    </property>
    
    <property name="descriptionOfMaterial" type="java.lang.String" column="DescriptionOfMaterial" length="120">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="text1" type="text" column="Text1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="text2" type="text" column="Text2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="number1" type="java.lang.Float" column="Number1" length="24">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="number2" type="java.lang.Float" column="Number2" length="24">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="yesNo1" type="boolean" column="YesNo1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="yesNo2" type="boolean" column="YesNo2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>


    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one name="agentSentTo" class="edu.ku.brc.specify.datamodel.Agent" cascade="save-update, lock, merge" outer-join="true">
        <meta attribute="field-description">Agent ID of organization material was sent to</meta>
        <column name="SentToOrganizationID"  not-null="true"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one name="agentCatalogedBy" class="edu.ku.brc.specify.datamodel.Agent" cascade="save-update, lock, merge" outer-join="true">
        <meta attribute="field-description">Agent ID of person who recorded  the exchange</meta>
        <column name="CatalogedByID"  not-null="true"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Shipment -->
    <!-- <many-to-one name="shipment" class="edu.ku.brc.specify.datamodel.Shipment"  outer-join="true" index="IX_ExchangeOutShipmentID">
        <meta attribute="field-description">Shipment information for the exchange</meta>
        <column name="ShipmentID"/>
    </many-to-one> -->
    
    <!-- bi-directional many-to-many association to Shipment -->
    <set name="shipments" table="exchangeout_shipment" inverse="true" lazy="true" cascade="save-update, lock, merge">
        <key>
            <column name="ExchangeOutID"/>
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.Shipment" column="ShipmentID"/>
    </set>  

</class>
</hibernate-mapping>
