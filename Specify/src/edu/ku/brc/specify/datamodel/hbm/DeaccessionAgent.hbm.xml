<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.DeaccessionAgent" table="deaccessionagents" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="deaccessionAgentsId" type="java.lang.Integer" column="DeaccessionAgentsID">
        <meta attribute="field-description"></meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>

    <property name="role" type="java.lang.String" column="Role" not-null="true" length="32" unique-key="IX_DeaccessionAgents">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Role agent played in deaccession</meta>
    </property>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampModified" type="java.sql.Timestamp" column="TimestampModified" length="23">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to AgentAddress -->
    <many-to-one name="agentAddress" class="edu.ku.brc.specify.datamodel.AgentAddress" not-null="true" outer-join="true" index="IX_DeaccessionAgentsAgentAddressID" unique-key="IX_DeaccessionAgents">
        <meta attribute="field-description">AgentAddressID for agent</meta>
        <column name="AgentAddressID"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Deaccession -->
    <many-to-one name="deaccession" class="edu.ku.brc.specify.datamodel.Deaccession" not-null="true" outer-join="true" cascade="none" index="IX_DeaccessionAgentsDeaccessionID" unique-key="IX_DeaccessionAgents">
        <meta attribute="field-description">Deaccession agent played role in</meta>
        <column name="DeaccessionID"/>
    </many-to-one>

</class>
</hibernate-mapping>
