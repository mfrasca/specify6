<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="edu.ku.brc.specify.datamodel.BorrowShipment" 
    table="borrowshipments"
	lazy="true"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="borrowshipments"
    </meta>

    <id
        name="borrowShipmentsId"
        type="java.lang.Integer"
        column="BorrowShipmentsID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="native"
            type="java.lang.Integer"
            column="BorrowShipmentsID"

        </meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native" />
    </id>
    
    <property
        name="remarks"
        type="text"
        column="Remarks"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
        </meta>    
    </property>
    
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
        not-null="true"
        update="false"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
            update="false"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="lastEditedBy"
        type="java.lang.String"
        column="LastEditedBy"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="LastEditedBy"
            length="50"
        </meta>    
    </property>

    <!-- Associations -->
		<!-- this was missing but existing in original mapping file - now replaced -->
    <!-- bi-directional many-to-one association to Shipment -->
    <many-to-one
        name="shipment"
        class="edu.ku.brc.specify.datamodel.Shipment"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="ShipmentID"         
        </meta>
        <column name="ShipmentID" />
    </many-to-one>
	
    <!-- bi-directional many-to-one association to Borrow -->
    <many-to-one
        name="borrow"
        class="edu.ku.brc.specify.datamodel.Borrow"
        not-null="true"
		outer-join="true"
		cascade="none"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
			cascade="none"
           @hibernate.column name="BorrowID"         
        </meta>
        <column name="BorrowID" />
    </many-to-one>

</class>
</hibernate-mapping>
