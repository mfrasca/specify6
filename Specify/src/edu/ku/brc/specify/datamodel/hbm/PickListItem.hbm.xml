<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
    <hibernate-mapping package="edu.ku.brc.specify.datamodel" >
        
        <class name="edu.ku.brc.specify.datamodel.PickListItem" table="picklistitems">
            
            <id name="pickListItemId" type="java.lang.Long" column="PickListItemID">
                <meta attribute="scope-set">public</meta>
                <meta attribute="scope-field">protected</meta>
                <generator class="native"/>
            </id>
            
            <property name="title" type="java.lang.String" column="Title" not-null="true" length="64">
                <meta attribute="scope-field">protected</meta>
            </property>
            
            <property name="value" type="java.lang.String" column="Value" not-null="true" length="64">
                <meta attribute="scope-field">protected</meta>
            </property>
            
            <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
                <meta attribute="scope-field">protected</meta>
                <meta attribute="field-description"></meta>    
            </property>
            
            <!-- Associations -->
            
            <!-- bi-directional many-to-one association to PickLIst not-null="true" -->
            <many-to-one name="pickList" class="edu.ku.brc.specify.datamodel.PickList" outer-join="true" cascade="save-update,lock,merge">
                <column name="PickListID"/>
            </many-to-one>
            
        </class>
        
    </hibernate-mapping>
    