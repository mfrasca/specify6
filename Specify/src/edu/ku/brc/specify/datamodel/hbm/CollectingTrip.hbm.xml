<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.CollectingTrip" table="collectingtrip" lazy="true">

    <id name="collectingTripId" type="java.lang.Long" column="CollectingTripID">
        <generator class="native"/>
    </id>

    <timestamp name="timestampModified" column="TimestampModified"/>

    <property name="remarks" type="java.lang.String" length="255" column="Remarks"/>
    
    <property name="startDate" type="calendar_date" column="StartDate"/>
    
    <property name="startDatePrecision" type="java.lang.Short" column="StartDatePrecision"/>
    
    <property name="startDateVerbatim" type="java.lang.String" length="50" column="StartDateVerbatim"/>
    
    <property name="endDate" type="calendar_date" column="EndDate"/>
    
    <property name="endDatePrecision" type="java.lang.Short" column="EndDatePrecision"/>
    
    <property name="endDateVerbatim" type="java.lang.String" length="50" column="EndDateVerbatim"/>
    
    <property name="startTime" type="java.lang.Short" column="StartTime"/>
    
    <property name="endTime" type="java.lang.Short" column="EndTime"/>
    
    <property name="timestampCreated" type="java.sql.Timestamp" length="23" column="TimestampCreated" not-null="true" update="false"/>
    
    <property name="lastEditedBy" type="java.lang.String" length="50" column="LastEditedBy"/>

    <!-- Associations -->
    
    <!-- bi-directional one-to-many association to CollectingEvent -->
    <set name="collectingEvents" inverse="true" cascade="save-update, lock, merge">
        <key column="CollectingTripID"/>
        <one-to-many class="edu.ku.brc.specify.datamodel.CollectingEvent"/>
    </set>	
    
    </class>
</hibernate-mapping>
