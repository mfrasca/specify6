<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

    <class name="edu.ku.brc.specify.datamodel.GeographyTreeDef" table="geographytreedef" lazy="true">
    <meta attribute="class-description" inherit="false"/>
        <meta attribute="implements">TreeDefinitionIface</meta>

        <id name="treeDefId" type="java.lang.Integer" column="TreeDefID">
            <meta attribute="scope-field">protected</meta>
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="field-description"></meta>
            <generator class="native"/>
        </id>

        <property name="name" type="java.lang.String" column="Name" length="64">
            <meta attribute="scope-field">protected</meta>
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="field-description"></meta>
        </property>

        <property name="remarks" type="java.lang.String" column="Remarks" length="255">
            <meta attribute="scope-field">protected</meta>
            <meta attribute="field-description"></meta>
        </property>
        
        <!-- Associations -->
        
        <set name="treeEntries" table="Geography" inverse="true" lazy="true">
            <meta attribute="field-description"></meta>
            <key column="TreeDefID"/>
            <one-to-many class="edu.ku.brc.specify.datamodel.Geography"/>
        </set>
        
        <set name="treeDefItems" table="GeographyTreeDefItem" inverse="true" lazy="false">
            <meta attribute="field-description"></meta>
            <key column="TreeDefID"/>
            <one-to-many class="edu.ku.brc.specify.datamodel.GeographyTreeDefItem"/>
        </set>
        
        <set name="collObjDefs" table="CollectionObjDef" inverse="true">
            <meta attribute="field-description"></meta>
            <key column="geographyTreeDefID"/>
            <one-to-many class="edu.ku.brc.specify.datamodel.CollectionObjDef"/>
        </set>

    </class>
    
</hibernate-mapping>
