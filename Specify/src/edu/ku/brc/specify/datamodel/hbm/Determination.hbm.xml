<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.Determination" table="determination" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="determinationId" type="java.lang.Integer" column="DeterminationID">
        <meta attribute="field-description">Primary key</meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>

    <property name="typeStatusName" type="java.lang.String" column="TypeStatusName" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">e.g. 'Holotype', 'Paratype'...</meta>
    </property>
    
    <property name="determinedDate" type="calendar_date" column="DeterminedDate">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="confidence" type="java.lang.String" column="Confidence" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Confidence of determination (value from PickList)</meta>
    </property>
    
    <property name="method" type="java.lang.String" column="Method" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Method of determination (value from PickList)</meta>
    </property>
    
    <property name="featureOrBasis" type="java.lang.String" column="FeatureOrBasis" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Body part, or characteristic used as the basis of a determination.</meta>
    </property>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="text1" type="text" column="Text1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="text2" type="text" column="Text2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="number1" type="java.lang.Float" column="Number1" length="24">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="number2" type="java.lang.Float" column="Number2" length="24">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Date/Time the record was created</meta>
    </property>
    
    <property name="timestampModified" type="java.sql.Timestamp" column="TimestampModified" length="23">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Date/Time the record was modified</meta>
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="yesNo1" type="boolean" column="YesNo1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="yesNo2" type="boolean" column="YesNo2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>



    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to DeterminationStatus -->
    <many-to-one name="status" class="edu.ku.brc.specify.datamodel.DeterminationStatus" not-null="true" outer-join="true">
        <meta attribute="scope-field">protected</meta>
        <column name="DeterminationStatusID"/>
    </many-to-one>

    <!-- bi-directional many-to-one association to Taxon -->
    <many-to-one name="taxon" class="edu.ku.brc.specify.datamodel.Taxon" not-null="true" outer-join="true">
	<meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <column name="TaxonID"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to CollectionObject -->
    <many-to-one name="collectionObject" class="edu.ku.brc.specify.datamodel.CollectionObject" not-null="true" outer-join="true" cascade="none">
	    <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <column name="CollectionObjectID"/>
    </many-to-one>	

    <!-- bi-directional one-to-many association to DeterminationCitation -->
    <set name="determinationCitations" lazy="true" inverse="true" cascade="delete">
        <meta attribute="field-description"></meta>
        <key>
            <column name="DeterminationID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.DeterminationCitation"/>
    </set>
	
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one name="determiner" class="edu.ku.brc.specify.datamodel.Agent" not-null="true" outer-join="true">
        <meta attribute="field-description">id of the Person making the determination</meta>
        <column name="DeterminerID"/>
    </many-to-one>

</class>
</hibernate-mapping>
