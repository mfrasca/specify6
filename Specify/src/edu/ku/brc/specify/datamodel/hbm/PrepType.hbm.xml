<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.PrepType" table="preptype" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="prepTypeId" type="java.lang.Integer" column="PrepTypeID">
      <meta attribute="scope-set">public</meta>
      <meta attribute="scope-field">protected</meta>
      <generator class="native"/>
    </id>
	
    <property name="name" type="java.lang.String" column="Name" length="32">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

	
	
	<!-- Associations -->
	
    <!-- bi-directional one-to-many association to Preparation -->
    <set name="preparations" lazy="true" inverse="true" cascade="none">
	<meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <key>
            <column name="PrepTypeID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.Preparation"/>
    </set>

    <!-- bi-directional one-to-many association to AttributeDef -->
    <set name="attributeDefs" lazy="true" inverse="true" cascade="delete">
	<meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <key>
            <column name="PrepTypeID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.AttributeDef"/>
    </set>

	
</class>
</hibernate-mapping>
