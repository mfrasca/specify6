-->

<class 
    name="edu.ku.brc.specify.datamodel.Loan" 
    table="loan"
	lazy="true"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="loan"
    </meta>

    <id
        name="loanId"
        type="java.lang.Integer"
        column="LoanID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="native"
            type="java.lang.Integer"
            column="LoanID"

        </meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native" />
    </id>

    <property
        name="loanNumber"
        type="java.lang.String"
        column="LoanNumber"
        not-null="true"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="LoanNumber"
            length="50"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="loanDate"
        type="calendar_date"
        column="LoanDate"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="LoanDate"
            length="10"
        </meta>    
    </property>
    
    <property name="currentDueDate"
        type="calendar_date"
        column="CurrentDueDate"
        length="10"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="CurrentDueDate"
            length="10"
        </meta>    
    </property>
    
    <property
        name="originalDueDate"
        type="calendar_date"
        column="OriginalDueDate"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="OriginalDueDate"
        </meta>    
    </property>
    
    <property
        name="dateClosed"
        type="calendar_date"
        column="DateClosed"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="DateClosed"
        </meta>    
    </property>
    
    <property
        name="category"
        type="java.lang.Byte"
        column="Category"
        length="3"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Category"
            length="3"
        </meta>    
    </property>
    
    <property
        name="remarks"
        type="text"
        column="Remarks"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
        </meta>    
    </property>
    
    <property
        name="text1"
        type="text"
        column="Text1"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Text1"
        </meta>    
    </property>
    
    <property
        name="text2"
        type="text"
        column="Text2"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Text2"
        </meta>    
    </property>
    
    <property
        name="number1"
        type="java.lang.Float"
        column="Number1"
        length="24"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Number1"
            length="24"
        </meta>    
    </property>
    
    <property
        name="number2"
        type="java.lang.Float"
        column="Number2"
        length="24"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Number2"
            length="24"
        </meta>    
    </property>
    
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
        not-null="true"
        update="false"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
            update="false"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="lastEditedBy"
        type="java.lang.String"
        column="LastEditedBy"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="LastEditedBy"
            length="50"
        </meta>    
    </property>
    
    <property
        name="closed"
        type="short"
        column="Closed"
    >
        <meta attribute="scope-field">protected</meta>
