        type="boolean"
        column="YesNo1"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="YesNo1"
        </meta>    
    </property>
    
    <property
        name="yesNo2"
        type="boolean"
        column="YesNo2"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
