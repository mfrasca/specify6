            column="YesNo2"
        </meta>    
    </property>



    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to LoanAgent -->
    <set
        name="loanAgents"
        lazy="true"
        inverse="true"
		cascade="delete"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="delete"

           @hibernate.collection-key
            column="LoanID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.LoanAgent"
        </meta>
        <key>
            <column name="LoanID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.LoanAgent"
        />
    </set>
    
    <!-- bi-directional one-to-many association to LoanPhysicalObject -->
    <set
        name="loanPhysicalObjects"
        lazy="true"
        inverse="true"
		cascade="delete"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="delete"

           @hibernate.collection-key
            column="LoanID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.LoanPhysicalObject"
        </meta>
        <key>
            <column name="LoanID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.LoanPhysicalObject"
        />
    </set>
    
    <!-- bi-directional many-to-one association to Shipment -->
    <many-to-one
        name="shipment"
        class="edu.ku.brc.specify.datamodel.Shipment"
        not-null="true"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="ShipmentID"         
        </meta>
        <column name="ShipmentID" />
    </many-to-one>

    <!-- bi-directional many-to-many association to ExternalResource -->
    <set
        name="externalResources"
        table="loan_extres"
        lazy="true"
        inverse="true"
		cascade="save-update">
        <key>
            <column name="LoanID" />
        </key>
        <many-to-many class="edu.ku.brc.specify.datamodel.ExternalResource" column="ExternalResourceID"/>
    </set>
    
</class>
</hibernate-mapping>
