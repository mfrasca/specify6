<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
    <hibernate-mapping package="edu.ku.brc.specify.datamodel" >
        
        <class name="edu.ku.brc.specify.datamodel.RecordSetItem" table="recordsetitems">
            
            <id name="recordSetItemId" type="java.lang.Long" column="RecordSetItemID">
                <meta attribute="scope-set">public</meta>
                <meta attribute="scope-field">protected</meta>
                <generator class="native"/>
            </id>
            
            <property name="RecordId" not-null="true" unique="false"/>
            
            <!-- Associations -->
            
            <!-- bi-directional many-to-one association to PickLIst not-null="true" -->
            <many-to-one name="recordSet" class="edu.ku.brc.specify.datamodel.RecordSet" outer-join="true" cascade="save-update,lock,merge">
                <column name="RecordSetID"/>
            </many-to-one>
            
        </class>
        
    </hibernate-mapping>
    