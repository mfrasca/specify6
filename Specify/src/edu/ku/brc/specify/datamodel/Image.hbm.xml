<class 
    name="edu.ku.brc.specify.datamodel.Image" 
    table="Image"
	lazy="true"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="Image"
    </meta>

    <id
        name="imageId"
        type="java.lang.Integer"
        column="ImageID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Integer"
            column="ImageID"

        </meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="assigned" />
    </id>

    <property
        name="madeDate"
        type="java.lang.Integer"
        column="MadeDate"
        length="10"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="MadeDate"
            length="10"
        </meta>    
    </property>
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
        </meta>    
    </property>
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
        </meta>    
    </property>
    <property
        name="lastEditedBy"
        type="java.lang.String"
        column="LastEditedBy"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="LastEditedBy"
            length="50"
        </meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-one association to CollectionObject -->
 <!--   <one-to-one
        name="collectionObject"
        class="edu.ku.brc.specify.datamodel.CollectionObject"
        outer-join="auto"
        constrained="true"
    >
        <meta attribute="field-description">
           @hibernate.one-to-one
            class="edu.ku.brc.specify.datamodel.CollectionObject"
            outer-join="auto"
            constrained="true"
        </meta>    
    </one-to-one>-->
	
    <!-- bi-directional one-to-one association to CollectionObj -->
    <one-to-one
        name="collectionObject"
        class="edu.ku.brc.specify.datamodel.CollectionObj"
        outer-join="auto"
        constrained="true"
		cascade="delete"
    >
        <meta attribute="field-description">
           @hibernate.one-to-one
            class="edu.ku.brc.specify.datamodel.CollectionObj"
            outer-join="auto"
            constrained="true"
			cascade="delete"
        </meta>    
    </one-to-one>	
	
    <!-- bi-directional one-to-many association to ImageCollectionObject -->
    <set
        name="imageCollectionObjects"
        lazy="true"
        inverse="true"
		cascade="delete"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="delete"

           @hibernate.collection-key
            column="ImageID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.ImageCollectionObject"
        </meta>
        <key>
            <column name="ImageID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.ImageCollectionObject"
        />
    </set>
    <!-- bi-directional one-to-many association to ImageAgent -->
    <set
        name="imageAgents"
        lazy="true"
        inverse="true"
		cascade="delete"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="delete"

           @hibernate.collection-key
            column="ImageID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.ImageAgent"
        </meta>
        <key>
            <column name="ImageID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.ImageAgent"
        />
    </set>
    <!-- bi-directional one-to-many association to ImageLocality -->
    <set
        name="imageLocalities"
        lazy="true"
        inverse="true"
		cascade="delete"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="delete"

           @hibernate.collection-key
            column="ImageID"

           @hibernate.collection-one-to-many
            class="edu.ku.brc.specify.datamodel.ImageLocality"
        </meta>
        <key>
            <column name="ImageID" />
        </key>
        <one-to-many 
            class="edu.ku.brc.specify.datamodel.ImageLocality"
        />
    </set>
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one
        name="agentByCopyrightOwnerId"
        class="edu.ku.brc.specify.datamodel.Agent"
        not-null="true"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="CopyrightOwnerID"         
        </meta>
        <column name="CopyrightOwnerID" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one
        name="agentByMadeById"
        class="edu.ku.brc.specify.datamodel.Agent"
        not-null="true"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="MadeByID"         
        </meta>
        <column name="MadeByID" />
    </many-to-one>

</class>
