    table="taxoncitation"
	lazy="true"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="taxoncitation"
    </meta>

    <id
        name="taxonCitationId"
        type="java.lang.Integer"
        column="TaxonCitationID"
