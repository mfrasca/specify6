            column="Number1"
            length="24"
        </meta>    
    </property>
    
    <property
        name="number2"
        type="java.lang.Float"
        column="Number2"
        length="24"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Number2"
            length="24"
        </meta>    
    </property>
    
    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
        not-null="true"
        update="false"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
            update="false"
            not-null="true"
        </meta>    
    </property>
    
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
