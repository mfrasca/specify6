        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
        </meta>    
    </property>
    
    <property
        name="text1"
        type="java.lang.String"
        column="Text1"
        length="300"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Text1"
            length="300"
        </meta>    
    </property>
    
    <property
        name="text2"
        type="java.lang.String"
        column="Text2"
        length="300"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Text2"
            length="300"
        </meta>    
    </property>
    
    <property
