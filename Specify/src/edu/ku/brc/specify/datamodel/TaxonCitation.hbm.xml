           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="ReferenceWorkID"         
        </meta>
        <column name="ReferenceWorkID" />
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Taxon -->
    <many-to-one
        name="Taxon"
        class="edu.ku.brc.specify.datamodel.Taxon"
        not-null="true"
		outer-join="true"
		cascade="none"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
			cascade="none"
           @hibernate.column name="TaxonID"         
        </meta>
