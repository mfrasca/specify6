<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="edu.ku.brc.specify.datamodel.ExtResAttrs" 
    table="extresattrs"
	lazy="true"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="extResattrs"
    </meta>
    <meta attribute="implements">AttrsSettableGettable</meta>

    <id
        name="extResAttrsId"
        type="java.lang.Integer"
        column="ExtResAttrsID"
    >
      <meta attribute="scope-set">public</meta>
      <meta attribute="scope-field">protected</meta>
      <generator class="native"/>
    </id>
	
    <property
        name="name"
        type="java.lang.String"
        column="Name"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Name"
            length="50"
        </meta>    
    </property>

    <property
        name="strValue"
        type="java.lang.String"
        column="StrValue"
        length="128"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="StrValue"
            length="128"
        </meta>    
    </property>

    <property
        name="intValue"
        type="java.lang.Integer"
        column="IntValue"
        length="10"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="IntValue"
            length="10"
        </meta>    
    </property>

    <property
        name="fieldType"
        type="java.lang.Short"
        column="FieldType"
        length="2"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="FieldType"
            length="2"
        </meta>    
    </property>

    <property
        name="unit"
        type="java.lang.Short"
        column="Unit"
        length="2"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Unit"
            length="2"
        </meta>    
    </property>

    <property
        name="timestampCreated"
        type="java.sql.Timestamp"
        column="TimestampCreated"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampCreated"
            length="23"
        </meta>    
    </property>
    
    <property
        name="timestampModified"
        type="java.sql.Timestamp"
        column="TimestampModified"
        length="23"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="TimestampModified"
            length="23"
        </meta>    
    </property>
	
    <property
        name="remarks"
        type="java.lang.String"
        column="Remarks"
        length="1073741823"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="Remarks"
            length="1073741823"
        </meta>
    </property>
	
	
	<!-- Associations -->
	
    <!-- bi-directional many-to-one association to ExternalResource -->
    <many-to-one
        name="externalResource"
        class="edu.ku.brc.specify.datamodel.ExternalResource"
        not-null="true"
		outer-join="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="ExternalResourceID"         
        </meta>
        <column name="ExternalResourceID" />
    </many-to-one>	
	
</class>
</hibernate-mapping>
