            <alias name="Attachment"/>
        </table>
    	<table name="CollectionObjectCitation">
    		<alias name="ReferenceWork"/>
    	</table>
    	
    	<table name="CollectingEvent">
	    	<table name="Collector" field="collectors">
             	<table name="Agent" abbrev="CtrAg">
             	</table>
        	</table>
    	</table>
    	
        <table name="Determination" field="determinations" title="Determinations">
            <table name="Taxon">
            	<treelevel rank="60" name="name"/>
            	<treelevel rank="100" name="name"/> 
            	<treelevel rank="140" name="name"/>
            	<table name="TaxonAttachment">
                	<alias name="Attachment"/>
            	</table>
                <table name="TaxonCitation">
                	<alias name="ReferenceWork"/>
                </table>
            </table>
            <table name="DeterminationCitation">
                <alias name="ReferenceWork"/>
            </table>
            <alias name="Agent" field="DeterminedBy"/>
        </table>
        
        <table name="PaleoContext" field="paleoContext">
    		<table name="GeologicTimePeriod"/>
    		<table name="LithoStrat"/>
    	</table>
        <alias name="Preparation" field="properation"/>
        <alias name="CollectingEvent" field="collectingEvent"/>
    </table>
    
    <table name="Locality">
    	<table name="Geography"/>
        <table name="LocalityNameAlias"/>
        <table name="LocalityAttachment">
        	<alias name="Attachment"/>
        </table>
        <table name="LocalityCitation">
            <alias name="ReferenceWork"/>
        </table>
    </table>
    
    <table name="Permit">
        <table name="PermitAttachment">
            <alias name="Attachment"/>
        </table>
    </table>

    <table name="Preparation">
        <table name="PrepType"/>
        <table name="PreparationAttachment">
            <table name="Attachment"/>
        </table>
        <table name="PreparationAttributes"/>
    </table>
    
    <table name="ReferenceWork">
        <table name="Journal"/>
        <table name="Author">
            <alias name="Agent"/>
        </table>
    </table>
    
