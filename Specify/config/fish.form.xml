        <viewdef
            type="form"
            name="Accession"
            class="edu.ku.brc.specify.datamodel.Accession"
            gettable="edu.ku.brc.specify.ui.forms.DataGetterForObj"
            settable="edu.ku.brc.specify.ui.forms.DataSetterForObj">
            <desc><![CDATA[The Accession form.]]></desc>
            <enableRules/>

            <columnDef>80px,3dlu,p,3dlu,80px,3dlu,p,3dlu,60px,3dlu,p,p:g</columnDef>
            <rowDef>p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,10dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p</rowDef>

            <rows>
                <row>
                    <cell type="separator" label="KU Accessions" colspan="12"/>
                </row>
                <row>
                    <cell type="label" labelfor="number" label="Number"/>
                    <cell type="field" name="number" isrequired="true" uitype="formattedtext" uifieldformatter="AccessionNumber" valtype="Focus"/>

                    <cell type="label" labelfor="status" label="Status"/>
                    <cell type="field" name="status"  uitype="combobox" picklist="AccessionStatus" isrequired="true" valtype="Focus"/>
                </row>
                <row>
                    <cell type="label" labelfor="type" label="Type"/>
                    <cell type="field" name="type" uitype="combobox" picklist="AccessionType" isrequired="true" valtype="Focus"/>

                    <cell type="label" labelfor="text1" label="Division"/>
                    <cell type="field" name="text1" uitype="combobox" initialize="Botany,Entomology,Herpetology,Ichthyology,Invertebrate Paleo,Invertebrate Zoology,Mammalogy,Ornithology,Paleobotany,Vertebrate Paleo" isrequired="true" valtype="Focus"/>

                </row>
                <row>
                    <cell type="label" labelfor="dateAccessioned" label="Accessioned"/>
                    <cell type="field" name="dateAccessioned" isrequired="true" uitype="formattedtext" uifieldformatter="Date" valtype="Focus"/>

                    <cell type="label" labelfor="dateReceived" label="Received"/>
                    <cell type="field" name="dateReceived" isrequired="true" uitype="formattedtext" uifieldformatter="Date" valtype="Focus"/>
                </row>
                <row>
                    <cell type="label" labelfor="text2" label="Description"/>
                    <cell type="field" name="text2" uitype="textarea" cols="60" rows="6" colspan="10"/>
                </row>
                <row>
                    <cell type="separator" label="Accession Agents" colspan="12"/>
                </row>
                <row>
                    <cell type="subview" viewname="AccessionAgent" name="accessionAgents" colspan="12"/>
                </row>

                <row>
                    <cell type="separator" label="Authorizations" colspan="12"/>
                </row>
                <row>
                    <cell type="subview" viewname="AccessionAuthorizations" name="accessionAuthorizations" colspan="12"/>
                </row>
                <row>
                    <cell type="separator" label="" colspan="12"/>
                </row>
                <row>
                    <cell type="label" labelfor="lastEditedBy" label="Edited By"/>
                    <cell type="field" name="lastEditedBy"/>

                    <cell type="label" labelfor="timestampModified" label="Last Edited"/>
                    <cell type="field" name="timestampModified" cols="20" colspan="2"/>
                </row>
            </rows>
        </viewdef>

