                    <cell type="separator" label="Accession Agents" colspan="12"/>
                </row>
                <row>
                    <cell type="subview" viewname="AccessionAgent" id="8" name=" accessionAgents" colspan="12"/>
                </row>

                <row>
                    <cell type="separator" label="Authorizations" colspan="12"/>
                </row>
                <row>
                    <cell type="subview" viewname="AccessionAuthorizations" id="9" name=" accessionAuthorizations" colspan="12"/>
                </row>
