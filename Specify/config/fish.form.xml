            settable="edu.ku.brc.specify.ui.forms.DataSetterForObj">
            <desc><![CDATA[The Accession form.]]></desc>
            <enableRules/>

            <columnDef>100px,3dlu,p,3dlu,100px,3dlu,p,3dlu,60px,3dlu,p,p:g</columnDef>
            <rowDef>p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,10dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p,2dlu,p</rowDef>

            <rows>
                <row>
                    <cell type="separator" label="KU Accessions" colspan="12"/>
                </row>
                <row>
                    <cell type="label" labelfor="1" label="Number"/>
                    <cell type="field" id="1" name="number" isrequired="true" uitype="formattedtext" uifieldformatter="AccessionNumber" valtype="Focus"/>

                    <cell type="label" labelfor="2" label="Status"/>
                    <cell type="field" id="2" name=" status"  uitype="combobox" picklist="AccessionStatus" isrequired="true" valtype="Focus"/>
                </row>
                <row>
