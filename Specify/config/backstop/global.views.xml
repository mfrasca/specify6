                    <cell type="label" labelfor="6" label="Date Renewed"/>
                    <cell type="field" id="6" name="renewalDate" uitype="formattedtext" uifieldformatter="Date" valtype="Changed"/>
                </row>
                
                <row>
                    <cell type="label" labelfor="7" label="Start Date"/>
