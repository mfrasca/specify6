            <detailView icon="CollectionObject">
                <sql>
                    <![CDATA[
 SELECT distinct 
 collectionobject.CollectionObjectID, 
 CatalogNumber, 
 CatalogedDate, 
 FullName, 
 %s 
 From collectionobject 
 Inner Join determination AS DT ON DT.CollectionObjectID = collectionobject.CollectionObjectID
 Inner Join determinationstatus ON DT.DeterminationStatusID = determinationstatus.DeterminationStatusID AND determinationstatus.Type = 1
 Inner Join taxon ON taxon.TaxonID = DT.TaxonID 
 WHERE DT.CollectionMemberID = COLMEMID AND 
 collectionobject.CollectionMemberID = COLMEMID AND 
 taxon.TaxonTreeDefID = TAXTREEDEFID AND 
 %s order by CatalogNumber asc]]>
                </sql>
                <captions>
                    <caption tableid="1" col="collectionobject.CollectionObjectID" visible="false"/>
                    <caption tableid="1" col="CatalogNumber" formatter="CatalogNumber"/>
                    <caption tableid="1" col="CatalogedDate"/>
                    <caption tableid="4" col="FullName" key="TaxonName"/>
                </captions>
            </detailView>
        </table>
        
