        <table id="113" tableid="52" name="OpenLoans" priority="1" color="30, 144, 255">
            <detailView icon="Loan">
                <sql>
                    <![CDATA[SELECT DISTINCT loan.LoanID, LoanNumber, LoanDate, CurrentDueDate, OriginalDueDate FROM loan INNER JOIN loanpreparation ON loan.LoanID = loanpreparation.LoanID
 INNER JOIN preparation ON loanpreparation.PreparationID = preparation.PreparationID
 INNER JOIN collectionobject ON preparation.CollectionObjectID = collectionobject.CollectionObjectID
 INNER JOIN collection ON collectionobject.CollectingEventID = collection.CollectionID
 INNER JOIN collectiontype ON collection.DisciplineID = collectiontype.DisciplineID
 INNER JOIN specifyuser ON collectiontype.SpecifyUserID = specifyuser.SpecifyUserID 
 WHERE specifyuser.SpecifyUserID = SPECIFYUSERID AND loan.CollectionMemberID = COLMEMID AND 
 (not (currentduedate is null)) AND (IsClosed = false or IsClosed is null) AND 
                    datediff(CURDATE(), currentduedate) > 0]]>
                </sql>
                <captions>
                    <caption tableid="52" col="loan.LoanID" visible="false"/>
                    <caption tableid="52" col="LoanNumber"/>
                    <caption tableid="52" col="LoanDate"/>
                    <caption tableid="52" col="CurrentDueDate"/>
                    <caption tableid="52" col="OriginalDueDate"/>
                </captions>
