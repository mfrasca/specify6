    <express>
    
        <table id="1" tableid="9" indexed="false" name="CollObjToDeterminer" title="Determiner" priority="1" color="30, 144, 144">
            <outofdate>
                <table name="determination" title="Determination"/>
            </outofdate>
            <index>
                <sql>
                    <![CDATA[SELECT ColFieldsDef FROM specifyuser INNER JOIN collectiontype ON specifyuser.SpecifyUserID = collectiontype.SpecifyUserID
	 INNER JOIN collection ON collectiontype.CollectionTypeID = collection.CollectionTypeID
	 INNER JOIN collectionobject ON collectionobject.CollectionID = collection.CollectionID
	 INNER JOIN determination ON collectionobject.CollectionObjectID = determination.CollectionObjectID
	 INNER JOIN taxon ON determination.TaxonID = taxon.TaxonID
	 INNER JOIN determinationstatus ON determination.DeterminationStatusID = determinationstatus.DeterminationStatusID
	 INNER JOIN agent ON determination.DeterminerID = agent.AgentID
     WHERE specifyuser.SpecifyUserID = SPECIFYUSERID and isCurrent = true]]>
                </sql>
                <cols>
                    <col pos="1" id="true">collectionobject.CollectionObjectID</col>
                    <col pos="2">taxon.FullName</col>
                    <col pos="3">taxon.CommonName</col>
                    <col pos="4">agent.LastName</col>
                    <col pos="5">agent.FirstName</col>
                    
                    <join pos="6" tableid="1">collectionobject.CollectionObjectID</join>
                    <join pos="7" tableid="9">determination.CollectionObjectID</join>
                    <join pos="8" tableid="5">agent.AgentID</join>
                    <join pos="9" tableid="9">determination.DeterminerID</join>
                    <join pos="10" tableid="9">determination.DeterminationStatusID</join>
                    <join pos="11" tableid="9">determination.TaxonID</join>
                    <join pos="12" tableid="4">taxon.TaxonID</join>
                </cols>
            </index>
            <detailView icon="Agent">
                <sql>
                    <![CDATA[SELECT * FROM (SELECT 
 collectionobject.CollectionObjectID 
 taxon.FullName,
 taxon.CommonName,
 agent.LastName,
 agent.FirstName,
 FROM
 collectionobject
 INNER JOIN determination ON determination.CollectionObjectID = collectionobject.CollectionObjectID
 INNER JOIN agent ON determination.DeterminerID = agent.AgentID
 INNER JOIN taxon ON determination.TaxonID = taxon.TaxonID
 INNER JOIN determinationstatus ON determination.DeterminationStatusID = determinationstatus.DeterminationStatusID
 WHERE determinationstatus.DeterminationStatusID = 1) AS tx
 WHERE tx.CollectionObjectID IN (%s)]]>
                </sql>
                <captions>
                    <caption col="CollectionObjectCatalogID" text="ID" visible="false"/>
                    <caption col="TaxonName" text="Taxon Name"/>
                    <caption col="CommonName" text="Common Name"/>
                    <caption col="LastName" text="Last Name"/>
                    <caption col="FirstName" text="First Name"/>
               </captions>
               <recordset tabletype="7" col="1"/>
            </detailView>
        </table>
    
        <table id="2" tableid="10" indexed="false" name="CollObToLocality" title="Localities in Collecting Events" priority="1" color="30, 144, 144">
            <outofdate>
                <table name="collectingevent" title="Collecting Event"/>
            </outofdate>
            <index>
                <sql>
                    <![CDATA[SELECT ColFieldsDef FROM collectingevent INNER JOIN locality ON collectingevent.LocalityID = locality.LocalityID]]>
                </sql>
                <cols>
                    <col pos="1">locality.LocalityName</col>
                    <col pos="2" key="startdate">StartDate</col>
                    <col pos="3" key="enddate">EndDate</col>
                    <col pos="4">locality.Latitude1</col>
                    <col pos="5">locality.Longitude1</col>
                    <col pos="6" id="true">collectingevent.CollectingEventID</col>
                    
                    <join pos="7" tableid="1">collectingevent.CollectingEventID</join>
                    <join pos="8" tableid="10">collectingevent.LocalityID</join>
                    <join pos="9" tableid="2">locality.LocalityID</join>
                </cols>
            </index>
            <detailView icon="ColEvent">
                <sql>
                    <![CDATA[SELECT locality.LocalityName, collectingevent.StartDate, collectingevent.EndDate, locality.Latitude1,
locality.Longitude1, collectingevent.CollectingEventID From collectingevent
Inner Join locality ON collectingevent.LocalityID = locality.LocalityID
where collectingevent.CollectingEventID in (%s) order by StartDate]]>
                </sql>
                <captions>
                    <caption col="LocalityName" text="Locality Name"/>
                    <caption col="StartDate" text="Start Date"/>
                    <caption col="EndDate" text="End Date"/>
                    <caption col="Latitude1" text="Latitude1"/>
                    <caption col="Longitude1" text="Longitude1"/>
                    <caption col="CollectingEventID" text="CollectingEventID" visible="false"/>
                </captions>
                <recordset tabletype="8" col="6"/>
            </detailView>
        </table>
            
        <table id="3" tableid="1" indexed="false" name="CollObject" title="Collection Object" priority="1" color="30, 144, 144">
            <outofdate>
                <table name="collectionobject" title="Collection Object"/>
                <table name="determination" title="Determination"/>
                <table name="taxon" title="Taxon"/>
            </outofdate>
            <index>
                <sql>
                    <![CDATA[SELECT distinct ColFieldsDef From collectionobject 
 Inner Join determination ON determination.CollectionObjectID = collectionobject.CollectionObjectID 
 Inner Join taxon ON taxon.TaxonID = determination.TaxonID]]>
                </sql>
                <cols>
                    <col pos="1" id="true">collectionobject.CollectionObjectID</col>
                    <col pos="2" formatter="CatalogNumber">collectionobject.CatalogNumber</col>
                    <col pos="3">collectionobject.CatalogedDate</col>
                    <col pos="4">taxon.FullName</col>
                    <join pos="5" tableid="9">determination.CollectionObjectID</join>
                    <join pos="6" tableid="1">collectionobject.CollectionObjectID</join>
                    <join pos="7" tableid="4">taxon.TaxonID</join>
                    <join pos="8" tableid="9">determination.TaxonID</join>
                </cols>
            </index>
            <detailView icon="ColObj">
                <sql>
                    <![CDATA[SELECT distinct collectionobject.CollectionObjectID, collectionobject.CatalogNumber, collectionobject.CatalogedDate, taxon.FullName From collectionobject Inner Join determination ON determination.CollectionObjectID = collectionobject.CollectionObjectID Inner Join taxon ON taxon.TaxonID = determination.TaxonID where collectionobject.CollectionObjectID in (%s) order by CatalogNumber asc]]>
                </sql>
                <captions>
                    <caption col="collectionobject.CollectionObjectID" text="CollectionObjectID" visible="false"/>
                    <caption col="collectionobject.CatalogNumber" text="Catalog Number" formatter="CatalogNumber"/>
                    <caption col="collectionobject.CatalogedDate" text="Cataloged Date"/>
                    <caption col="taxon.FullName" text="Taxon Name"/>
                </captions>
                <recordset tabletype="1" col="1"/>
            </detailView>
        </table>
        
        <table id="4" tableid="4" indexed="false" name="GeoToTaxon" title="Geography To Taxon" priority="1" color="30, 144, 144">
            <outofdate/>
            <index>
                <sql>
                    <![CDATA[SELECT 
    taxon.TaxonID,
    geography.FullName,
    locality.LocalityName,
    collectingevent.StartDate,
    collectionobject.CatalogNumber,
    collectionobject.CatalogedDate,
    taxon.FullName
 FROM geography INNER JOIN locality ON geography.GeographyID = locality.GeographyID
     INNER JOIN collectingevent ON locality.LocalityID = collectingevent.LocalityID
     INNER JOIN collectionobject ON collectingevent.CollectingEventID = collectionobject.CollectingEventID
     INNER JOIN determination ON collectionobject.CollectionObjectID = determination.CollectionObjectID
     INNER JOIN taxon ON determination.TaxonID = taxon.TaxonID
     INNER JOIN determinationstatus ON determination.DeterminationStatusID = determinationstatus.DeterminationStatusID
 WHERE determinationstatus.IsCurrent = true]]>
                </sql>
                <cols>
                    <col pos="1" id="true">taxon.TaxonID</col>
                    <col pos="2">geography.FullName</col>
                    <col pos="3">locality.LocalityName</col>
                    <col pos="4">collectingevent.StartDate</col>
                    <col pos="5">collectionobject.CatalogNumber</col>
                    <col pos="6">collectionobject.CatalogedDate</col>
                    <col pos="7">taxon.FullName</col>
                    <join pos="8" tableid="2">locality.LocalityID</join>
                    <join pos="9" tableid="9">determination.CollectionObjectID</join>
                    <join pos="10" tableid="1">collectionobject.CollectionObjectID</join>
                    <join pos="11" tableid="4">taxon.TaxonID</join>
                    <join pos="12" tableid="9">determination.TaxonID</join>
                    <join pos="13" tableid="88">determinationstatus.DeterminationStatusID</join>
                    <join pos="14" tableid="88">geography.GeographyID</join>
                </cols>
            </index>
            <detailView icon="Taxon">
                <sql>
                    <![CDATA[SELECT 
    taxon.TaxonID,
    geography.FullName,
    locality.LocalityName,
    collectingevent.StartDate,
    collectionobject.CatalogNumber,
    collectionobject.CatalogedDate,
    taxon.FullName
 FROM geography INNER JOIN locality ON geography.GeographyID = locality.GeographyID
     INNER JOIN collectingevent ON locality.LocalityID = collectingevent.LocalityID
     INNER JOIN collectionobject ON collectingevent.CollectingEventID = collectionobject.CollectingEventID
     INNER JOIN determination ON collectionobject.CollectionObjectID = determination.CollectionObjectID
     INNER JOIN taxon ON determination.TaxonID = taxon.TaxonID
     INNER JOIN determinationstatus ON determination.DeterminationStatusID = determinationstatus.DeterminationStatusID
 WHERE determinationstatus.IsCurrent = true]]>
                </sql>
                <captions>
                    <caption col="taxon.TaxonID" text="taxon.TaxonID" visible="false"/>
                    <caption col="geography.FullName" text="Geography" formatter="CatalogNumber"/>
                    <caption col="locality.LocalityName" text="Locality Name"/>
                    <caption col="collectingevent.StartDate" text="Start Date"/>
                    <caption col="collectionobject.CatalogNumber" text="Cataloged Number"/>
                    <caption col="collectionobject.CatalogedDate" text="Cataloged Date"/>
                    <caption col="taxon.FullName" text="Taxon Name"/>
                </captions>
                <recordset tabletype="4" col="1"/>
            </detailView>
        </table>
        
    </express>
    
