        <table id="9" tableid="1" indexed="false" name="OtherSynsCollObjs" priority="1" color="30, 144, 144">
            <joins>
                <join tableid="1" primary="true">co.CollectionObjectID</join>
                <join tableid="9">d.CollectionObjectID</join>
                <join tableid="4">deterTax.TaxonID</join>
                <join tableid="4">acceptTax.TaxonID</join>
                <join tableid="4">oldTax.TaxonID</join>
            </joins>
            <detailView icon="Taxon">
                <sql>
                    <![CDATA[SELECT DISTINCT co.CollectionObjectID, co.CatalogNumber, deterTax.FullName, acceptTax.FullName, oldTax.FullName, %s 
 FROM collectionobject co 
 INNER JOIN determination d ON d.CollectionObjectID = co.CollectionObjectID 
 INNER JOIN taxon deterTax ON d.TaxonID = deterTax.TaxonID 
 INNER JOIN taxon acceptTax ON acceptTax.TaxonID = deterTax.AcceptedID 
 INNER JOIN taxon oldTax ON oldTax.AcceptedID = acceptTax.TaxonID AND oldTax.TaxonID != deterTax.TaxonID 
 WHERE collectionobject.CollectionMemberID = COLMEMID AND %s]]>
                </sql>
                <captions>
                    <caption tableid="1" col="co.CollectionObjectID" visible="false"/>
                    <caption tableid="1" col="co.CatalogNumber"      formatter="CatalogNumber"/>
                    <caption tableid="4" col="deterTax.FullName"     key="OldTaxonOnDeter"/>
                    <caption tableid="4" col="acceptTax.FullName"    key="AcceptedTaxonName"/>
                    <caption tableid="4" col="oldTax.FullName"       key="OtherOldTaxonName"/>
                </captions>
            </detailView>
        </table>

        <table id="10" tableid="1" indexed="false" name="CurrCollObject" priority="1" color="30, 144, 144">
