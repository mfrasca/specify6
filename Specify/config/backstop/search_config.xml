    <express>
    
        <table id="1" tableid="4" indexed="false" name="CollObjToDeterminer" priority="1" color="30, 144, 144">
            <joins>
                <join tableid="5" primary="true">agent.AgentID</join>
                <join tableid="1">collectionobject.CollectionObjectID</join>
                <join tableid="9">determination.DeterminerID</join>
                <join tableid="9">determination.DeterminationStatusID</join>
                <join tableid="4">taxon.TaxonID</join>
            </joins>
            <detailView icon="Agent">
                <sql>
                    <![CDATA[SELECT 
 taxon.TaxonID,
 taxon.FullName,
 taxon.CommonName,
 agent.LastName,
 agent.FirstName,
 agent.AgentType,
 %s
 FROM
 collectionobject
 INNER JOIN determination ON determination.CollectionObjectID = collectionobject.CollectionObjectID
 INNER JOIN agent ON determination.DeterminerID = agent.AgentID
 INNER JOIN taxon ON determination.TaxonID = taxon.TaxonID
 INNER JOIN determinationstatus ON determination.DeterminationStatusID = determinationstatus.DeterminationStatusID
 WHERE determination.CollectionMemberID = COLMEMID AND determinationstatus.Type = 1 AND %s]]>
                </sql>
                <captions>
                    <caption tableid="4" col="taxon.TaxonID" visible="false"/>
                    <caption tableid="4" col="TaxonName"  key="TaxonName"/>
                    <caption tableid="4" col="CommonName"/>
                    <caption tableid="4" key="Determiner" dataobjformatter="Agent">
                        <col name="agent.LastName"  field="lastName"/>
                        <col name="agent.FirstName" field="firstName"/>
                        <col name="agent.AgentType" field="agentType"/>                        
                    </caption>
                    
               </captions>
            </detailView>
        </table>
    
        <table id="2" tableid="1" indexed="false" name="CollObToLocality" priority="1" color="30, 144, 144">
            <joins>
                <join tableid="1" primary="true">collectingevent.CollectingEventID</join>
                <join tableid="2">locality.LocalityID</join>
            </joins>
            <detailView icon="ColEvent">
                <sql>
                    <![CDATA[SELECT 
 collectingevent.CollectingEventID, 
 locality.LocalityName, 
 collectingevent.StartDate, 
 collectingevent.EndDate, 
 locality.Latitude1,
 locality.Longitude1, 
 %s 
 From 
 collectingevent
 Inner Join locality ON collectingevent.LocalityID = locality.LocalityID
 WHERE collectingevent.CollectionMemberID = COLMEMID AND %s order by StartDate]]>
                </sql>
                <captions>
                    <caption tableid="1" col="CollectingEventID" visible="false"/>
                    <caption tableid="2" col="LocalityName"/>
                    <caption tableid="10" col="StartDate"/>
                    <caption tableid="10" col="EndDate"/>
                    <caption tableid="2" col="Latitude1"/>
                    <caption tableid="2" col="Longitude1"/>
                </captions>
            </detailView>
        </table>
            
        <table id="3" tableid="1" indexed="false" name="CollObject" priority="1" color="30, 144, 144">
            <joins>
                <join tableid="1" primary="true">collectionobject.CollectionObjectID</join>
                <join tableid="4">taxon.TaxonID</join>
            </joins>
            <detailView icon="CollectionObject">
                <sql>
                    <![CDATA[
 SELECT distinct 
 collectionobject.CollectionObjectID, 
 CatalogNumber, 
 CatalogedDate, 
 FullName, 
 %s 
 From collectionobject 
 Inner Join determination ON determination.CollectionObjectID = collectionobject.CollectionObjectID
 Inner Join taxon ON taxon.TaxonID = determination.TaxonID 
 WHERE collectionobject.CollectionMemberID = COLMEMID AND %s order by CatalogNumber asc]]>
                </sql>
                <captions>
                    <caption tableid="1" col="collectionobject.CollectionObjectID" visible="false"/>
                    <caption tableid="1" col="CatalogNumber" formatter="CatalogNumber"/>
                    <caption tableid="1" col="CatalogedDate"/>
                    <caption tableid="4" col="FullName" key="TaxonName"/>
                </captions>
            </detailView>
        </table>
        
        <table id="4" tableid="4" indexed="false" name="GeoToTaxon" priority="1" color="30, 144, 144">
            <joins>
                <join tableid="4" primary="true">taxon.TaxonID</join>
                <join tableid="2">locality.LocalityID</join>
                <join tableid="1">collectionobject.CollectionObjectID</join>
                <join tableid="88">determinationstatus.DeterminationStatusID</join>
                <join tableid="3">geography.GeographyID</join>
            </joins>
            <detailView icon="Taxon">
                <sql>
                    <![CDATA[SELECT 
 taxon.TaxonID,
 geography.FullName,
 locality.LocalityName,
 CatalogNumber,
 CatalogedDate,
 StartDate,
 taxon.FullName,
 %s
 FROM geography INNER JOIN locality ON geography.GeographyID = locality.GeographyID
 INNER JOIN collectingevent ON locality.LocalityID = collectingevent.LocalityID
 INNER JOIN collectionobject ON collectingevent.CollectingEventID = collectionobject.CollectingEventID
 INNER JOIN determination ON collectionobject.CollectionObjectID = determination.CollectionObjectID
 INNER JOIN taxon ON determination.TaxonID = taxon.TaxonID
 INNER JOIN determinationstatus ON determination.DeterminationStatusID = determinationstatus.DeterminationStatusID
 WHERE collectionobject.CollectionMemberID = COLMEMID AND determinationstatus.Type = 1 AND %s]]>
                </sql>
                <captions>
                    <caption tableid="4" col="taxon.TaxonID"         visible="false"/>
                    <caption tableid="3" col="geography.FullName"    key="Geography"/>
                    <caption tableid="2" col="locality.LocalityName"/>
                    <caption tableid="1" col="CatalogNumber" formatter="CatalogNumber"/>
                    <caption tableid="1" col="CatalogedDate"/>
                    <caption tableid="10" col="StartDate"/>
                    <caption tableid="4" col="taxon.FullName"        key="TaxonName"/>
                </captions>
            </detailView>
        </table>

        <table id="5" tableid="1" indexed="false" name="ColObjCollectors" priority="1" color="30, 144, 144">
            <joins>
                <join tableid="1" primary="true">collectionobject.CollectionObjectID</join>
                <join tableid="4">taxon.TaxonID</join>
                <join tableid="2">collectingevent.CollectingEventID</join>
                <join tableid="88">determinationstatus.DeterminationStatusID</join>
                <join tableid="5">agent.AgentID</join>
            </joins>
            <detailView icon="ColectionObject">
                <sql>
                    <![CDATA[
 SELECT 
 collectionobject.CollectionObjectID,
 CatalogNumber,
 CatalogedDate,
 StartDate,
 taxon.FullName,
 agent.LastName,
 agent.FirstName,
 agent.AgentType,
 collector.OrderNumber,
 %s
 FROM collectionobject INNER JOIN determination ON collectionobject.CollectionObjectID = determination.CollectionObjectID
 INNER JOIN collectingevent ON collectionobject.CollectingEventID = collectingevent.CollectingEventID
 INNER JOIN collector ON collectingevent.CollectingEventID = collector.CollectingEventID
 INNER JOIN agent ON collector.AgentID = agent.AgentID
 INNER JOIN taxon ON determination.TaxonID = taxon.TaxonID
 INNER JOIN determinationstatus ON determination.DeterminationStatusID = determinationstatus.DeterminationStatusID 
 WHERE collectionobject.CollectionMemberID = COLMEMID AND determinationstatus.Type = 1 AND %s ORDER BY collectionobject.CollectionObjectID, collector.OrderNumber]]>
                </sql>
                <captions>
                    <caption tableid="1"  col="collectionobject.CollectionObjectID" visible="false"/>
                    <caption tableid="1"  col="CatalogNumber"   formatter="CatalogNumber"/>
                    <caption tableid="1"  col="CatalogedDate"/>
                    <caption tableid="10" col="StartDate"/>
                    <caption tableid="4"  col="taxon.FullName"  key="TaxonName"/>
                    <caption tableid="10" key="Collectors">
                        <agg class="edu.ku.brc.specify.datamodel.Collector" aggregator="Collector" ordercol="collector.OrderNumber"/>
                        <subobject class="edu.ku.brc.specify.datamodel.Agent" fieldname="agent"/>
                        <col name="agent.LastName"  field="lastName"/>
                        <col name="agent.FirstName" field="firstName"/>
                        <col name="agent.AgentType" field="agentType"/>                        
                    </caption>
               </captions>
            </detailView>
        </table>
        
