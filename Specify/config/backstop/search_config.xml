                <join tableid="4">t.TaxonID</join>
                <join tableid="4">t2.TaxonID</join>
            </joins>
            <detailView icon="Taxon">
                <sql>
                    <![CDATA[SELECT DISTINCT co.CollectionObjectID, co.CatalogNumber, t.FullName, t2.FullName, %s FROM 
 collectionobject co INNER JOIN determination d ON d.CollectionObjectID = co.CollectionObjectID 
 INNER JOIN taxon t ON d.TaxonID = t.TaxonID 
 INNER JOIN taxon t2 ON t2.TaxonID = t.AcceptedID 
 WHERE t.TaxonTreeDefID = TAXTREEDEFID AND co.CollectionMemberID = COLMEMID AND %s]]>
                </sql>
                <captions>
                    <caption tableid="1" col="co.CollectionObjectID" visible="false"/>
                    <caption tableid="1" col="co.CatalogNumber" formatter="CatalogNumber"/>
                    <caption tableid="4" col="t.FullName"   key="DeterminationName"/>
                    <caption tableid="4" col="t2.FullName"  key="AcceptedTaxonName"/>
                </captions>
            </detailView>
        </table>
        
