  <format name="Agent" title="Agent" class="edu.ku.brc.specify.datamodel.Agent" default="true">
    <switch single="false" field="agentType">
      <fields value="0">
        <field>lastName</field>
      </fields>
      <fields value="1">
        <field>lastName</field>
        <field sep=", ">firstName</field>
      </fields>
      <fields value="2">
        <field>lastName</field>
      </fields>
      <fields value="3">
