<?xml version="1.0" encoding="UTF-8"?>
<formatters xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <format
        name="Agent"
        class="edu.ku.brc.specify.datamodel.Agent"
        default="true"
        >
        <switch single="false" field="agentType">
            <fields value="0">
                <field>name</field>
            </fields>
            <fields value="1">
                <field>lastName</field>
                <field sep=", " >firstName</field>
            </fields>
            <fields value="2">
                <field>name</field>
            </fields>
        </switch>
    </format>
    
    <format
        name="CollectionObject"
        class="edu.ku.brc.specify.datamodel.CollectionObject"
        default="true"
        >
        <switch single="true">
            <fields>
                <field format="%.0f" type="float">catalogNumber</field>
            </fields>
        </switch>
    </format>

    <format
        name="Permit"
