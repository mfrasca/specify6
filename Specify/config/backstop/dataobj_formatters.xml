    <aggregators>
        <aggregator name="AccessionAuthorization" 
            class="edu.ku.brc.specify.datamodel.AccessionAuthorization"
            separator="; " 
            count="" ending="" 
            format="AccessionAuthorization" 
            default="true"/>
        
        <aggregator name="AccessionAgent"
                    class="edu.ku.brc.specify.datamodel.AccessionAgent" 
                    separator="; " 
                    count="" 
                    ending="" 
                    format="AccessionAgent" 
                    default="true"/>   
        
        <aggregator name="Preparation"
                    class="edu.ku.brc.specify.datamodel.Preparation" 
                    separator="; " 
                    count="" 
                    ending="" 
                    format="Preparation" 
                    default="true"/>   
        
        <aggregator name="Determination"
                    class="edu.ku.brc.specify.datamodel.Determination" 
                    separator="; " 
                    count="1" 
                    ending="" 
                    format="Determination" 
                    default="true"/>   
        
        <aggregator name="Collector"
                    class="edu.ku.brc.specify.datamodel.Collector" 
                    separator="; " 
                    count="" 
                    ending="" 
                    format="Collector" 
                    default="true"
                    orderfieldname="orderNumber"/>   
    </aggregators>
    
     
