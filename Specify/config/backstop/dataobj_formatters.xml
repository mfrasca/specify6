  <aggregators>
    <aggregator name="AccessionAgent"
                class="edu.ku.brc.specify.datamodel.AccessionAgent"
                default="true"
                separator="; "
               
                count="null"
                formatName="AccessionAgent"
               
               />

    <aggregator name="AccessionAuthorization"
