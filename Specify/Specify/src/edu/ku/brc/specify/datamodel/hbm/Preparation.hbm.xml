<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.Preparation" table="preparation" lazy="true">
    <meta attribute="class-description" inherit="false"/>

    <id name="preparationId" type="java.lang.Long" column="PreparationID">
        <meta attribute="field-description"></meta>
        <meta attribute="scope-field">protected</meta>
        <generator class="native"/>
    </id>
	
    <timestamp name="timestampModified" column="TimestampModified">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">Versioning TimeStamp</meta>
    </timestamp>
    
    <!--
    <property
        name="preparationMethod"
        type="java.lang.String"
        column="PreparationMethod"
        length="50"
    >
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    -->
        
    <property name="text1" type="text" column="Text1">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
    
    <property name="text2" type="text" column="Text2">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">User definable</meta>
    </property>
        
    <property name="count" type="java.lang.Integer" column="Count" length="10">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description">The number of objects (specimens, slides, pieces) prepared</meta>
    </property>
    
    <property name="storageLocation" type="java.lang.String" column="StorageLocation" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="remarks" type="text" column="Remarks">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
        
    <property name="preparedDate" type="calendar_date" column="PreparedDate">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>

    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>
    
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>    
    </property>


    <!-- Associations -->
         
     <!-- bi-directional one-to-many association to LoanPhysicalObject -->
    
    <set name="loanPhysicalObjects" lazy="true" inverse="true" cascade="none">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <key>
            <column name="PreparationID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.LoanPhysicalObject"/>
    </set>
  
    <!-- bi-directional one-to-many association to PreparationAttr -->
    <set name="attrs" lazy="true" inverse="true" cascade="all">
        <meta attribute="scope-field">protected</meta>
        <meta attribute="field-description"></meta>
        <key>
            <column name="PreparationID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.PreparationAttr"/>
    </set>
    
    <!-- bi-directional many-to-one association to PrepType -->
    <many-to-one name="prepType" class="edu.ku.brc.specify.datamodel.PrepType" not-null="true" outer-join="true">
        <meta attribute="field-description"></meta>
        <column name="PrepTypeID"/>
    </many-to-one>
  
    <!-- bi-directional many-to-one association to CollectionObject -->
    <many-to-one name="collectionObject" class="edu.ku.brc.specify.datamodel.CollectionObject" not-null="true" outer-join="true" cascade="none">
        <meta attribute="field-description"></meta>
        <column name="CollectionObjectID"/>
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Agent -->
    <many-to-one name="preparedByAgent" class="edu.ku.brc.specify.datamodel.Agent" not-null="true" outer-join="true">
        <meta attribute="field-description"></meta>
        <column name="PreparedByID"/>
    </many-to-one>
        
    <!-- bi-directional many-to-one association to Location -->
    <many-to-one name="location" class="edu.ku.brc.specify.datamodel.Location" not-null="false" outer-join="auto">
        <meta attribute="field-description"></meta>
        <column name="LocationID"/>
    </many-to-one>
        
    
    <!-- bi-directional one-to-many association to Attachment -->
    <set name="attachments" inverse="true" cascade="save-update">
        <key column="PreparationID"/>
        <one-to-many class="edu.ku.brc.specify.datamodel.Attachment"/>
    </set>
    
</class>
</hibernate-mapping>
