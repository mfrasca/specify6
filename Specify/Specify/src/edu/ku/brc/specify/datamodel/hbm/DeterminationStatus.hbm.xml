<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>

<class name="edu.ku.brc.specify.datamodel.DeterminationStatus" table="determinationstatus" lazy="true">

    <id name="determinationStatusId" type="java.lang.Long" column="DeterminationStatusID">
        <generator class="native"/>
    </id>
    
    <timestamp name="timestampModified" column="TimestampModified"/>
        
    <property name="name" type="java.lang.String" column="Name" length="64"/>
    <property name="remarks" type="java.lang.String" column="Remarks" length="255"/>
    <property name="timestampCreated" type="java.sql.Timestamp" column="TimestampCreated" length="23" not-null="true" update="false"/>
    <property name="lastEditedBy" type="java.lang.String" column="LastEditedBy" length="50"/>

    <!-- Associations -->

    <!-- bi-directional one-to-many association to DeterminationCitation -->
    <set name="determinations" lazy="true" inverse="true" cascade="none">
        <key>
            <column name="DeterminationStatusID"/>
        </key>
        <one-to-many class="edu.ku.brc.specify.datamodel.Determination"/>
    </set>

</class>
</hibernate-mapping>
